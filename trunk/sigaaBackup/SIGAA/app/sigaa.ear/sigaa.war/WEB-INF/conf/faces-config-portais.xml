		<from-view-id>*</from-view-id>

		<navigation-case>
			<from-outcome>validacao</from-outcome>
			<to-view-id>/prodocente/producao/validacao.jsp</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>artigos</from-outcome>
