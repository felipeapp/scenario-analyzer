		</navigation-case>

		<navigation-case>
			<from-outcome>apresentacaoeventos</from-outcome>
			<to-view-id>
				/prodocente/producao/ApresentacaoEmEvento/form.jsp
			</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>audiovisuais</from-outcome>
			<to-view-id>
				/prodocente/producao/AudioVisual/form.jsp
			</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>bancas</from-outcome>
			<to-view-id>/prodocente/producao/Banca/form.jsp</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>maquetesprototiposoutros</from-outcome>
			<to-view-id>
				/prodocente/producao/MaquetePrototipoOutro/form.jsp
			</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>participacaocolegiadocomissoes</from-outcome>
			<to-view-id>
				/prodocente/producao/ParticipacaoColegiadoComissao/form.jsp
			</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>participacaocomissaoorgeventos</from-outcome>
			<to-view-id>
				/prodocente/producao/ParticipacaoComissaoOrgEventos/form.jsp
			</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>participacaosociedades</from-outcome>
			<to-view-id>
				/prodocente/producao/ParticipacaoSociedade/form.jsp
			</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>patentes</from-outcome>
			<to-view-id>
				/prodocente/producao/Patente/form.jsp
			</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>premiorecebidos</from-outcome>
			<to-view-id>
				/prodocente/producao/PremioRecebido/form.jsp
			</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>publicacaoeventos</from-outcome>
			<to-view-id>
				/prodocente/producao/PublicacaoEvento/form.jsp
			</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>textodidaticos</from-outcome>
			<to-view-id>
				/prodocente/producao/TextoDidatico/form.jsp
			</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>textodiscussoes</from-outcome>
			<to-view-id>
				/prodocente/producao/TextoDiscussao/form.jsp
			</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>visitacientificas</from-outcome>
			<to-view-id>
				/prodocente/producao/VisitaCientifica/form.jsp
			</to-view-id>
		</navigation-case>


	</navigation-rule>

</faces-config>