<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN" "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">
<faces-config>


	<!--  Agenda -->
	<managed-bean>
		<managed-bean-name>today</managed-bean-name>
		<managed-bean-class>java.util.Date</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>scheduleModel</managed-bean-name>
		<managed-bean-class>
			org.apache.myfaces.custom.schedule.model.SimpleScheduleModel
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>mode</property-name>
			<value>1</value>
		</managed-property>
		<managed-property>
			<property-name>selectedDate</property-name>
			<value>#{today}</value>
		</managed-property>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>agenda</managed-bean-name>
		<managed-bean-class>
			br.ufrn.sigaa.portal.agenda.jsf.AgendaMBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>model</property-name>
			<value>#{scheduleModel}</value>
		</managed-property>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>compromisso</managed-bean-name>
		<managed-bean-class>
			br.ufrn.sigaa.portal.agenda.jsf.CompromissoMBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>model</property-name>
			<value>#{scheduleModel}</value>
		</managed-property>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>configuracoesAgenda</managed-bean-name>
		<managed-bean-class>
			br.ufrn.sigaa.portal.agenda.jsf.ConfiguracoesAgendaMBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>model</property-name>
			<value>#{scheduleModel}</value>
		</managed-property>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>scheduleThemeChoices</managed-bean-name>
		<managed-bean-class>java.util.TreeMap</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
		<map-entries>
			<map-entry>
				<key>default</key>
				<value>default</value>
			</map-entry>
			<map-entry>
				<key>evolution</key>
				<value>evolution</value>
			</map-entry>
			<map-entry>
				<key>outlookxp</key>
				<value>outlookxp</value>
			</map-entry>
		</map-entries>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>randomColorEntryRenderer</managed-bean-name>
		<managed-bean-class>
			br.ufrn.sigaa.portal.agenda.jsf.RandomColorScheduleEntryRenderer
		</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>


	<managed-bean>
		<managed-bean-name>menuDocente</managed-bean-name>
		<managed-bean-class>
			br.ufrn.sigaa.portal.jsf.MenuDocenteMBean
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>menuDiscente</managed-bean-name>
		<managed-bean-class>
			br.ufrn.sigaa.portal.jsf.MenuDiscenteMBean
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>

	<!-- PORTAL PÚBLICO -->
	<managed-bean>
		<managed-bean-name>portalPublicoDocente</managed-bean-name>
		<managed-bean-class>
			br.ufrn.sigaa.portal.jsf.PortalPublicoDocenteMBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	
	
	<managed-bean>
		<managed-bean-name>portalTurma</managed-bean-name>
		<managed-bean-class>
			br.ufrn.sigaa.ava.jsf.PortalTurmaMBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>perfilDocente</managed-bean-name>
		<managed-bean-class>
			br.ufrn.sigaa.portal.jsf.PerfilServidorMBean
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>perfilDiscente</managed-bean-name>
		<managed-bean-class>
			br.ufrn.sigaa.portal.jsf.PerfilDiscenteMBean
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>perfilTutor</managed-bean-name>
		<managed-bean-class>
			br.ufrn.sigaa.portal.jsf.PerfilTutorMBean
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>noticiaPortal</managed-bean-name>
		<managed-bean-class>
			br.ufrn.sigaa.portal.jsf.NoticiaPortalMBean
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>


<!--    #####################################################################   -->

	<!--  Navegações de Ensino -->
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>consolidaTurma</from-outcome>
			<to-view-id>
				/ensino/consolidacao/selecionaTurma.jsf
			</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>planoCurso</from-outcome>
			<to-view-id>
				/ensino/selecionaTurmaPlanoCurso.jsf
			</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>portalDocente</from-outcome>
			<to-view-id>
				/portais/docente/docente.jsf
			</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>portalDiscente</from-outcome>
			<to-view-id>
				/portais/discente/discente.jsf
			</to-view-id>
		</navigation-case>


	</navigation-rule>

	<!--  Navegações de Coordenação -->

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>coordenadorLato</from-outcome>
			<to-view-id>/lato/coordenador.jsf</to-view-id>
		</navigation-case>

	</navigation-rule>

	<navigation-rule>
