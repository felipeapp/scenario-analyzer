		<action path="/verMenuEnsino" type="br.ufrn.sigaa.ensino.struts.MenuEnsinoAction">
			<forward name="sucesso" path="/WEB-INF/jsp/menus/menu_principal.jsp" />
		</action>

		<action path="/verMenuTecnico" type="br.ufrn.sigaa.ensino.tecnico.struts.EntrarTecnicoAction">
			<forward name="sucesso" path="/ensino/menus/menu_tecnico.jsf" redirect="true" />
		</action>
		
		<action path="/verMenuFormacaoComplementar" type="br.ufrn.sigaa.ensino.tecnico.struts.EntrarFormacaoComplementarAction">
