		<action path="/verMenuProgramaAtualizacaoPedagogica" type="br.ufrn.sigaa.apedagogica.struts.EntrarProgramaAtualizacaoPedagogicaAction">
			<forward name="sucesso" path="/apedagogica/menu.jsf" redirect="true"/>
