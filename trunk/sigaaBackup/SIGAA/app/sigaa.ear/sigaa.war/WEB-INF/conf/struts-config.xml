		<action path="/verPortalPublico" 		forward="/public/index.jsp"/>
		<action path="/verTelaLogin" 			forward="/WEB-INF/jsp/include/login.jsp"/>
		<action path="/validarUsuario" 			forward="/verMenuPrincipal.do" />
		<action path="/caixaPostal"			 	forward="/WEB-INF/jsp/caixa_postal/caixa_postal.jsp"/>

		<action path="/teste" type="br.ufrn.sigaa.arq.struts.TestAction"/>

