			<forward name="sucesso" path="/monitoria/index.jsf" redirect="true"/>
		</action>

		<action path="/verMenuExtensao" type="br.ufrn.sigaa.extensao.struts.EntrarExtensaoAction">
			<forward name="sucesso" path="/extensao/menu.jsf" redirect="true"/>
			<forward name="menu_ta" path="/extensao/menu_ta.jsf" redirect="true"/>
		</action>

		<action path="/verMenuPrincipal" type="br.ufrn.sigaa.struts.MenuAction">
			<forward name="sucesso" path="/WEB-INF/jsp/menus/menu_principal.jsp" />
			<forward name="popup" path="/WEB-INF/jsp/menus/menu_principal_popup.jsp" />
		</action>

		<action path="/verMenuAdministracao" type="br.ufrn.sigaa.struts.MenuAdministracao">
			<forward name="sucesso" path="/administracao/index.jsf?aba=cadastro"  redirect="true"/>
