		<action path="/verPortalFamiliar" type="br.ufrn.sigaa.portal.struts.EntrarPortalFamiliar">
			<forward name="sucesso" path="/portais/familiar/index.jsf" redirect="true"/>
		</action>

		<action path="/verMenuRelacoesInternacionais" type="br.ufrn.sigaa.ensino.internacionalizacao.struts.EntrarRelacoesInternacionaisAction">
			<forward name="sucesso" path="/relacoes_internacionais/menu.jsf" redirect="true"/>
			<forward name="sucessoTradutor" path="/relacoes_internacionais/menu_tradutor.jsf" redirect="true"/>
		</action>		
					
    </action-mappings>

    <message-resources parameter="Mensagens" />

</struts-config>
