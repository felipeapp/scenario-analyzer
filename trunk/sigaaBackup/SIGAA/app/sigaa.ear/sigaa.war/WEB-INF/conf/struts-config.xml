		</action>
		
		<action path="/verPortalRelatorios" type="br.ufrn.sigaa.struts.PortalRelatoriosAction">
			<forward name="sucesso" path="/portais/relatorios/menu.jsf" redirect="true" />
		</action>				

		<action path="/logar" type="br.ufrn.sigaa.struts.LoginActions" name="loginForm"
					 input="/WEB-INF/jsp/include/login.jsp" parameter="dispatch" validate="true">
			<forward name="sucesso" 			path="/verMenuPrincipal.do" />
			<forward name="falha" 				path="/WEB-INF/jsp/include/login.jsp" />
			<forward name="menu_mobile" 		path="/mobile/menu.jsf" />
			<forward name="caixaPostal" 		path="/mensagem/showFolder.do" />
			<forward name="logarComo" 			path="/administracao/usuario/logar_como.jsf" />
			<forward name="avisoBrowser" 		path="/public/navegador.jsp" />
			<forward name="alterarSenha" 		path="/WEB-INF/jsp/geral/senha.jsp" />
			<forward name="progresso" 			path="/progresso.jsf" redirect="true"/>
		</action>

		<action path="/mobile/logonMobile" type="br.ufrn.sigaa.mobile.jsf.LogonMobileAction" parameter="dispatch" validate="false">
			<forward name="logonMobile" path="/mobile/login.jsp"/>
			<forward name="menuMobile" path="/mobile/menu.jsf" redirect="true"/>
			<forward name="selecionaVinculo" path="/mobile/escolhaVinculoMobile.jsf" redirect="true"/>
		</action>	
			
		<action path="/enviarAquivo" type="br.ufrn.sigaa.arq.struts.EnviarArquivoAction" name="loginForm"
					 input="/WEB-INF/jsp/include/login.jsp" parameter="dispatch" validate="false">
			<forward name="sucesso" 			path="/verMenuPrincipal.do" />
			<forward name="falha" 				path="/WEB-INF/jsp/include/login.jsp" />
		</action>

		<action path="/enviarAquivo" type="br.ufrn.sigaa.arq.struts.EnviarArquivoAction"
				parameter="dispatch" validate="false">
		</action>

		<action path="/escolhaVinculo" type="br.ufrn.sigaa.arq.struts.EscolhaVinculoAction"
				parameter="dispatch" validate="false">
			<forward name="vinculos" 			path="/vinculos.jsf" redirect="true" />
			<forward name="sucesso" 			path="/verMenuPrincipal.do" redirect="true"/>
			<forward name="caixaPostal" 		path="/abrirCaixaPostal.jsf" redirect="true"/>
			<forward name="progresso" 			path="/progresso.jsf" redirect="true"/>
		</action>

		<action path="/paginaInicial" type="br.ufrn.sigaa.arq.struts.PaginaInicialUsuarioAction" validate="false">
			<forward name="sucesso" 			path="/telasPosSelecaoVinculos.jsf" redirect="true" />
			<forward name="falha" 				path="/WEB-INF/jsp/include/login.jsp" />
			<forward name="caixaPostal" 		path="/abrirCaixaPostal.jsf" redirect="true" />
		</action>

		<action path="/public/historicoExterno" type="br.ufrn.sigaa.ensino.struts.GerarHistoricoAcessoExternoAction" validate="false"/>

		<action path="/verMenuRegistroDiplomas" type="br.ufrn.sigaa.diploma.struts.EntrarDiplomaAction">
			<forward name="sucesso" path="/diplomas/menu_geral.jsf" redirect="true"/>
		</action>
		
		<action path="/verMenuConveniosEstagio" type="br.ufrn.sigaa.estagio.struts.EntrarConveniosEstagio">
			<forward name="sucesso" path="/estagio/menu.jsf" redirect="true"/>
