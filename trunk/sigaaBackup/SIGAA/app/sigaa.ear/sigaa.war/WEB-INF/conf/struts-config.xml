			<forward name="geral" path="/graduacao/geral.jsf" redirect="true"/>
			<forward name="secretaria" path="/graduacao/secretaria.jsf" redirect="true"/>
			<forward name="departamento" path="/graduacao/departamento.jsf" redirect="true"/>
