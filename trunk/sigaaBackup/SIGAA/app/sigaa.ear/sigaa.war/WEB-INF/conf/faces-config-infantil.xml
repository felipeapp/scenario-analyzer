<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN" "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">

<faces-config>

	<managed-bean>
		<managed-bean-name>matriculaInfantil</managed-bean-name>
		<managed-bean-class>br.ufrn.sigaa.ensino.infantil.jsf.MatriculaInfantilMBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>menuInfantil</from-outcome>
			<to-view-id>/WEB-INF/jsp/menus/menu_infantil.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>matriculaInfantil</from-outcome>
			<to-view-id>/ensino/infantil/matricula/matriculaInfantil.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
