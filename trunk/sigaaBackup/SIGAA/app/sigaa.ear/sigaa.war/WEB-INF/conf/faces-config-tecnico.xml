<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN" "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">

<faces-config>

	<managed-bean>
		<managed-bean-name>especializacaoTurma</managed-bean-name>
		<managed-bean-class>br.ufrn.sigaa.ensino.tecnico.jsf.EspecializacaoTurmaMBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>relatoriosTecnico</managed-bean-name>
		<managed-bean-class>br.ufrn.sigaa.ensino.tecnico.jsf.RelatoriosTecnicoMBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>declaracaoTecnico</managed-bean-name>
		<managed-bean-class>br.ufrn.sigaa.ensino.tecnico.jsf.DeclaracaoTurmasMinistradasMBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>menuTecnico</from-outcome>
			<to-view-id>/ensino/menus/menu_tecnico.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>alunoTurma</from-outcome>
			<to-view-id>/ensino/tecnico/matricula/matriculaAluno.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>alunoModulo</from-outcome>
			<to-view-id>/ensino/tecnico/matricula/matriculaAluno.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>turmaTurma</from-outcome>
			<to-view-id>/ensino/tecnico/matricula/matriculaTurma.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>turmaModulo</from-outcome>
			<to-view-id>/ensino/tecnico/matricula/matriculaTurma.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>turmasSemestre</from-outcome>
			<to-view-id>/ensino/tecnico/matricula/turmasSemestre.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
