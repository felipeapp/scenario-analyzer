	<!-- Usado no menu de extensão para acessar a página de login da área interna -->
	<url-mapping id="acessarArea">
		<pattern>/link/public/extensao/acessarAreaInscrito</pattern>
		<view-id>#{logonCursosEventosExtensaoMBean.redirectTelaLoginCursosEventosExtensao}</view-id>
	</url-mapping>
	
	<!-- Quando a pessoa acessar o endereço abaixo, vai setar os valores do codigoAcesso  e idCadastroParticipante no MBean e chamar o métedo confirmarCadastroParticipante para confirmar o cadastro. -->
	<url-mapping id="confirmarCadastroCursosEventoExtensao">
		<pattern>/link/public/extensao/confirmarCadastro/#{cadastroParticipanteAtividadeExtensaoMBean.codigoAcessoConfirmacaoInscricao}/#{cadastroParticipanteAtividadeExtensaoMBean.idCadastroParticipanteNaoConfirmado}</pattern>
		<view-id>#{cadastroParticipanteAtividadeExtensaoMBean.confirmarCadastroParticipante}</view-id>
	</url-mapping>
	
	
	<!-- Quando a pessoa acessar o endereço abaixo, vai setar os valores do codigoAcesso  e idCadastroParticipante e senha gerada no MBean e chamar o métedo confirmarCadastroParticipante para confirmar a alteração de senha. -->
	<url-mapping id="confirmarAlteracaoSenhaCursosEventoExtensao">
		<pattern>/link/public/extensao/confirmarAlteracaoSenha/#{cadastroParticipanteAtividadeExtensaoMBean.codigoAcessoConfirmacaoInscricao}/#{cadastroParticipanteAtividadeExtensaoMBean.idCadastroParticipanteNaoConfirmado}</pattern>
		<view-id>#{cadastroParticipanteAtividadeExtensaoMBean.confirmarAlteracaoSenhaAcesso}</view-id>
	</url-mapping>
	
	<url-mapping id="assinaturaDigitalGrupoPesq">
		<pattern>/link/public/extensao/visualizacaoAcaoExtensao/#{inscricaoParticipanteAtividadeMBean.idAtividadeSelecionada}</pattern>
		<view-id>#{inscricaoParticipanteAtividadeMBean.visualizarDadosAcaoCursoEvento}</view-id>
	</url-mapping>
	
	<!--      REDIRECIONAMENTOS DE OUVIDORIA     -->
	
	
	<!-- Quando a pessoa acessar o endereço abaixo, vai setar os valores do codigoAcesso e idInteressadoNaoAutenticado e senha gerada no MBean e chamar o método confirmarConfirmarCodigoAcesso para confirmar a o recebimento do e-mail. -->
	<url-mapping id="confirmarCodigoAcessoOuvidoria">
		<pattern>/link/public/ouvidoria/confirmarCodigoAcessoOuvidoria/#{esclarecimentoOuvidoria.codigoAcesso}/#{esclarecimentoOuvidoria.idInteressadoNaoAutenticado}</pattern>
		<view-id>#{esclarecimentoOuvidoria.confirmarCodigoAcesso}</view-id>
	</url-mapping>
	
