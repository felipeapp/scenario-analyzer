	<!--              REDIRECIONAMENTOS DE  EXTENSÃO     -->
	
	
	<!-- Usado no menu de extensão para acessar a página aberta para quem não está logado que lista os cursos e eventos
	<url-mapping id="visualizaCursosEventos">
		<pattern>/link/public/extensao/visualizaCurosEventosPublico</pattern>
