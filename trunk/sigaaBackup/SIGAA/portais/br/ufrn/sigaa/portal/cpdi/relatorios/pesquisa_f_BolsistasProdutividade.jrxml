	<parameter name="analitico" isForPrompting="true" class="java.lang.Boolean">
		<defaultValueExpression ><![CDATA[new Boolean(true)]]></defaultValueExpression>
	</parameter>
	<parameter name="anoInicio" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(2000)]]></defaultValueExpression>
	</parameter>
	<parameter name="anoFim" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(2007)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT B.PERIODO_INICIO,B.PERIODO_FIM,B.ID_TIPO_BOLSA,T.DESCRICAO,S.ID_UNIDADE,P.NOME
   FROM PRODOCENTE.BOLSA_OBTIDA B
  INNER JOIN PRODOCENTE.PRODUCAO   PRO ON B.id_bolsa_obtida=PRO.id_producao 
  INNER JOIN RH.SERVIDOR           S ON S.ID_SERVIDOR=PRO.ID_SERVIDOR
  INNER JOIN COMUM.PESSOA          P ON P.ID_PESSOA=S.ID_PESSOA
  INNER JOIN PRODOCENTE.TIPO_BOLSA T ON T.ID_TIPO_BOLSA=B.ID_TIPO_BOLSA
  WHERE (
 		 (
  		  (EXTRACT(YEAR FROM B.PERIODO_INICIO) >= $P{anoInicio})
          AND
  		  (EXTRACT(YEAR FROM B.PERIODO_INICIO) <= $P{anoFim})
         ) 
         AND
 		 (
  		  (EXTRACT(YEAR FROM B.PERIODO_FIM) >= $P{anoInicio})
          AND
  		  (EXTRACT(YEAR FROM B.PERIODO_FIM) <= $P{anoFim})
         ) 
        ) 
    AND S.ID_UNIDADE=$P{unidade}
    AND T.PRODUTIVIDADE
  ORDER BY B.PERIODO_INICIO,T.DESCRICAO]]></queryString>

