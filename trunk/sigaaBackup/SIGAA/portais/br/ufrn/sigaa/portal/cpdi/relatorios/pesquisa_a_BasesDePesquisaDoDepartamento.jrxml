	<queryString><![CDATA[SELECT DISTINCT S.ID_UNIDADE,G.NOME
  FROM PESQUISA.GRUPO_PESQUISA G
 INNER JOIN PESQUISA.LINHA_PESQUISA L ON L.ID_GRUPO_PESQUISA=G.ID_GRUPO_PESQUISA
 INNER JOIN PESQUISA.PROJETO_PESQUISA PP ON PP.ID_LINHA_PESQUISA=L.ID_LINHA_PESQUISA
 INNER JOIN PROJETOS.PROJETO P ON P.ID_PROJETO=PP.ID_PROJETO
  LEFT JOIN RH.SERVIDOR S ON S.ID_SERVIDOR=G.ID_COORDENADOR
 WHERE S.ID_UNIDADE=$P{unidade}
   AND P.ATIVO 
 ORDER BY G.NOME]]></queryString>

