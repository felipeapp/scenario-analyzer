	<queryString><![CDATA[/* f-Número de professores em funções administrativas */
SELECT DISTINCT U.ID_UNIDADE,P.NOME,A.DESCRICAO,
       CASE WHEN D.GERENCIA='T' THEN 'TITULAR'
            WHEN D.GERENCIA='S' THEN 'SUBSTITUTO'
            ELSE 'NAO INFORMADO' 
       END AS GERENCIA
  FROM RH.SERVIDOR S
 INNER JOIN COMUM.PESSOA  P        ON P.ID_PESSOA=S.ID_PESSOA
 INNER JOIN COMUM.UNIDADE U        ON U.ID_UNIDADE=S.ID_UNIDADE
 INNER JOIN RH.DESIGNACAO  D        ON D.ID_SERVIDOR/*_TITULAR*/=S.ID_SERVIDOR 
 INNER JOIN RH.ATIVIDADE   A        ON A.ID_ATIVIDADE=D.ID_ATIVIDADE
 WHERE S.ID_UNIDADE=$P{unidade}
   AND S.ID_SITUACAO IN (1,12,19,20)
   AND ((D.FIM IS NULL) OR (D.FIM>CAST('NOW' AS DATE)))
/*   AND (
        (
           (EXTRACT(YEAR FROM CAST('NOW' AS DATE))>=EXTRACT(YEAR FROM D.INICIO)) 
           AND 
           (EXTRACT(YEAR FROM CAST('NOW' AS DATE))<=EXTRACT(YEAR FROM D.FIM))
        )
        OR
        (D.FIM IS NULL)
        )
*/
   AND S.ID_CATEGORIA=1 /*DOCENTE*/
ORDER BY P.NOME]]></queryString>

