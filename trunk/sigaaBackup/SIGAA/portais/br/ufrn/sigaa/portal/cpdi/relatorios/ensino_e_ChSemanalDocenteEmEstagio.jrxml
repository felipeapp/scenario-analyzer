	<queryString><![CDATA[SELECT U.ID_UNIDADE,S.ID_SERVIDOR,P.NOME, SUM(E.CH_DOCENTE_SEMANAL) as CH_DOCENTE_SEMANAL
  FROM PRODOCENTE.ESTAGIO E 
 INNER JOIN RH.SERVIDOR    S ON S.ID_SERVIDOR=E.ID_SERVIDOR
 INNER JOIN COMUM.PESSOA  P ON P.ID_PESSOA=S.ID_PESSOA
  LEFT JOIN COMUM.UNIDADE U ON U.ID_UNIDADE=S.ID_UNIDADE
  LEFT JOIN RH.FORMACAO    F ON F.ID_FORMACAO=S.ID_FORMACAO
 WHERE  S.ID_UNIDADE=$P{unidade}
   AND  S.ID_CATEGORIA=1 /*DOCENTE*/
   AND  EXTRACT(YEAR FROM PERIODO_INICIO)>= $P{ano}
   AND  EXTRACT(YEAR FROM PERIODO_FIM) <= $P{ano}
 GROUP BY U.ID_UNIDADE,S.ID_SERVIDOR,P.NOME
  ORDER BY P.NOME]]></queryString>

