	<queryString><![CDATA[SELECT ID_UNIDADE,NOME,CASE WHEN REGIME_TRABALHO='20' THEN '20'
                            WHEN REGIME_TRABALHO='40' THEN '40'
                            WHEN REGIME_TRABALHO='99' THEN 'DE'
                            ELSE REGIME_TRABALHO
                        END,
                        TO_CHAR(ADMISSAO,'DD/MM/YYYY') AS ADMISSAO,
                        TO_CHAR(DATA_APOSENTADORIA,'DD/MM/YYYY') AS DATA_APOSENTADORIA,
                        DENOMINACAO,ID_GRUPO_OCORRENCIA
  FROM ( 
		SELECT U.ID_UNIDADE,P.NOME, 
               CAST(S.REGIME_TRABALHO AS VARCHAR(2)) AS REGIME_TRABALHO,S.ADMISSAO,
               CASE WHEN F.DENOMINACAO='DESCONHECIDA' THEN 'GRADUADO'
                    ELSE F.DENOMINACAO
               END AS DENOMINACAO, 
               (SELECT OC.ID_GRUPO_OCORRENCIA 
			      FROM FUNCIONAL.associacao_grupo_ocorrencia OC
			   	inner join FUNCIONAL.OCORRENCIA_FUNCIONAL ofu on ofu.id_ocorrencia = OC.id_ocorrencia
			  	LEFT JOIN FUNCIONAL.OCORRENCIA O ON O.ID_OCORRENCIA=OC.ID_OCORRENCIA
			 	WHERE ofu.ID_SERVIDOR=S.ID_SERVIDOR 
			   AND OC.ID_GRUPO_OCORRENCIA=2 LIMIT 1
		       ) AS ID_GRUPO_OCORRENCIA,
               (SELECT MIN(DATA) 
                  FROM FUNCIONAL.APOSENTADORIA_PREVISAO AP
                 WHERE AP.ID_SERVIDOR=S.ID_SERVIDOR
               ) AS DATA_APOSENTADORIA   
		  FROM RH.SERVIDOR S
		 INNER JOIN COMUM.PESSOA  P ON P.ID_PESSOA=S.ID_PESSOA
		 LEFT  JOIN COMUM.UNIDADE U ON U.ID_UNIDADE=S.ID_UNIDADE
         LEFT  JOIN RH.FORMACAO    F ON F.ID_FORMACAO=P.ID_TIPO_FORMACAO
		 WHERE  S.ID_UNIDADE=$P{unidade}
		   AND S.ID_CATEGORIA=1 /*DOCENTE*/
		   AND S.ID_SITUACAO IN (1,20) /*ATIVO/CELETISTA*/
		 ) A
   WHERE ((A.ID_GRUPO_OCORRENCIA<>2) OR (A.ID_GRUPO_OCORRENCIA IS NULL))
   AND DATA_APOSENTADORIA <= NOW()
   ORDER BY NOME,REGIME_TRABALHO]]></queryString>

