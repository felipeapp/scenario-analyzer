	<queryString><![CDATA[SELECT ID_UNIDADE,ID_SERVIDOR,NOME,REGIME_TRABALHO,
TO_CHAR(ADMISSAO,'DD/MM/YYYY') AS ADMISSAO,DENOMINACAO,ID_GRUPO_OCORRENCIA,
TO_CHAR(DATA_OCORRENCIA,'DD/MM/YYYY') AS DATA_OCORRENCIA,
TO_CHAR(DATA_ULTIMA_ADMISSAO,'DD/MM/YYYY') AS DATA_ULTIMA_ADMISSAO FROM (
SELECT $P{unidade} as ID_UNIDADE,S.ID_SERVIDOR,P.NOME, 
               CAST(S.REGIME_TRABALHO AS VARCHAR(2)) AS REGIME_TRABALHO,S.ADMISSAO,
               CASE WHEN F.DENOMINACAO='DESCONHECIDA' THEN 'GRADUADO'
                    ELSE F.DENOMINACAO
               END AS DENOMINACAO,
               (		SELECT OC.ID_GRUPO_OCORRENCIA 
		      FROM FUNCIONAL.associacao_grupo_ocorrencia OC
		 inner join FUNCIONAL.OCORRENCIA_FUNCIONAL ofu on ofu.id_ocorrencia = OC.id_ocorrencia
		 LEFT JOIN FUNCIONAL.OCORRENCIA O ON O.ID_OCORRENCIA=OC.ID_OCORRENCIA
		 WHERE ofu.ID_SERVIDOR=S.ID_SERVIDOR 
		           AND OC.ID_GRUPO_OCORRENCIA IN (2,5,11,12,13,33)
                 ORDER BY DATA DESC LIMIT 1
		       ) AS ID_GRUPO_OCORRENCIA,
               (SELECT * FROM (
                  SELECT OFU.DATA
		      FROM FUNCIONAL.associacao_grupo_ocorrencia OC
		 inner join FUNCIONAL.OCORRENCIA_FUNCIONAL ofu on ofu.id_ocorrencia = OC.id_ocorrencia
		 LEFT JOIN FUNCIONAL.OCORRENCIA O ON O.ID_OCORRENCIA=OC.ID_OCORRENCIA
		 WHERE ofu.ID_SERVIDOR=S.ID_SERVIDOR 
			   AND OC.ID_GRUPO_OCORRENCIA IN (2,5,11,12,13,33)	
                     UNION
                     SELECT I.DATA_INATIVIDADE
                      FROM FUNCIONAL.INATIVIDADE I
                     WHERE I.ID_SERVIDOR = S.ID_SERVIDOR
                     UNION  
                     SELECT O.OBITO_DATA
                     FROM FUNCIONAL.OBITO O
                     WHERE O.ID_SERVIDOR=S.ID_SERVIDOR
                 ) A ORDER BY 1 DESC LIMIT 1
		       ) AS DATA_OCORRENCIA,
               (SELECT MAX(S.ADMISSAO) 
		 FROM RH.SERVIDOR S 
        WHERE ((S.ID_UNIDADE=$P{unidade}) OR ($P{unidade} IN (SELECT ID_UNIDADE 
                                                                  FROM FUNCIONAL.INATIVIDADE
                                                                 WHERE ID_SERVIDOR=S.ID_SERVIDOR
                                                           )
                                          )
              )                            
          AND S.ID_CATEGORIA=1
		  AND ID_TIPO_REGIME_JURIDICO IN (1,2) 
               ) AS DATA_ULTIMA_ADMISSAO
		  FROM RH.SERVIDOR S
		 INNER JOIN COMUM.PESSOA  P ON P.ID_PESSOA=S.ID_PESSOA
		 LEFT  JOIN COMUM.UNIDADE U ON U.ID_UNIDADE=S.ID_UNIDADE
         LEFT  JOIN RH.FORMACAO    F ON F.ID_FORMACAO=P.ID_TIPO_FORMACAO
        WHERE ((S.ID_UNIDADE=$P{unidade}) OR ($P{unidade} IN (SELECT ID_UNIDADE 
                                                                  FROM FUNCIONAL.INATIVIDADE
                                                                 WHERE ID_SERVIDOR=S.ID_SERVIDOR
                                                           )
                                          )
              )                            
		   AND S.ID_CATEGORIA=1 --DOCENTE
           AND ID_TIPO_REGIME_JURIDICO<>4 
              ) A
WHERE
DATA_OCORRENCIA>= (
-- ULTIMA DATA DE ADMISSAO
SELECT MAX(S.ADMISSAO) 
	 FROM RH.SERVIDOR S 
        WHERE S.ID_UNIDADE=$P{unidade}        
          AND S.ID_CATEGORIA=1
	  AND ID_TIPO_REGIME_JURIDICO IN (1,2) 
--	  AND (ID_SITUACAO IN (1,20) --ATIVO/CELETISTA
)
ORDER BY NOME]]></queryString>

