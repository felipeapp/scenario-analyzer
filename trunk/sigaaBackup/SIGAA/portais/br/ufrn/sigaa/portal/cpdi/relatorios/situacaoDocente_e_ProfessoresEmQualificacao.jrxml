	</parameter>
	<queryString><![CDATA[SELECT DISTINCT * FROM (
SELECT S.ID_UNIDADE,P.NOME/*,TO_CHAR(A.INICIO,'DD/MM/YYYY') AS INICIO,TO_CHAR(A.FIM,'DD/MM/YYYY') AS FIM*/,
       CASE WHEN A.ID_OCORRENCIA IN (30382,30442,30452,30672,31002,31132,31142,31332,31592,31612,39992,40132,40152,40162,40182,40192,40252,180261,180271,180281,180441,180621,180641,180811,180831,180841,180921,180931,182511,182531,301144,301244,301594)
                 THEN 'SAÚDE'
            WHEN A.ID_OCORRENCIA IN (31622,31632,31702,31712,31722,31733,180381,180391,180401,301634,301704,301714,302594,302604,302614,302624,302634,302644,302654)
                 THEN 'QUALIFICAÇÃO'
            WHEN A.ID_OCORRENCIA IN (10142,31522,31642,31652,301034,301044)
                 THEN 'CESSÃO'
            WHEN A.ID_OCORRENCIA IN (10142,31522,31602,31642,31652,301034,301044)
                 THEN 'OUTROS'
            ELSE 'NAO_CONSIDERAR'
       END AS TIPO     
  FROM FUNCIONAL.AUSENCIA A
 INNER JOIN RH.SERVIDOR S ON S.ID_SERVIDOR=A.ID_SERVIDOR
 INNER JOIN COMUM.PESSOA   P ON P.ID_PESSOA=S.ID_PESSOA
 WHERE S.ID_UNIDADE=$P{unidade}
   AND S.ID_CATEGORIA = 1
   AND S.ID_ATIVO IN (1,7)
   AND ((A.FIM IS NULL) OR (A.FIM>CAST('NOW' AS DATE)))
/*   AND (
        (EXTRACT(YEAR FROM CAST('NOW' AS DATE))>=EXTRACT(YEAR FROM A.INICIO)) 
       AND 
        ((EXTRACT(YEAR FROM CAST('NOW' AS DATE))<=EXTRACT(YEAR FROM A.FIM)) OR (A.FIM IS NULL))
       )
*/
)B
WHERE TIPO<>'NAO_CONSIDERAR'
ORDER BY B.ID_UNIDADE,TIPO,B.NOME]]></queryString>

