	<parameter name="anoFim" isForPrompting="false" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(2007)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT T.ID_DEPARTAMENTO AS ID_UNIDADE,EXTRACT(YEAR FROM T.DATA_PUBLICACAO) AS ANO,T.TITULO,
       CASE 
          WHEN ID_DISCENTE IS NULL THEN ORIENTANDO
          ELSE (
				 SELECT P.NOME 
				   FROM PUBLIC.DISCENTE D
				  INNER JOIN COMUM.PESSOA P ON P.ID_PESSOA=D.ID_PESSOA
				  WHERE D.ID_DISCENTE=T.ID_DISCENTE LIMIT 1
               )
       END AS DISCENTE
       ,P.NOME AS PROFESSOR
  FROM PRODOCENTE.TESE_ORIENTADA T
  LEFT JOIN  RH.SERVIDOR   S ON S.ID_SERVIDOR=T.ID_SERVIDOR
  INNER JOIN COMUM.PESSOA P ON P.ID_PESSOA=S.ID_PESSOA
  
 WHERE (
        (EXTRACT(YEAR FROM T.DATA_PUBLICACAO) >= $P{anoInicio})
        AND 
        (EXTRACT(YEAR FROM T.DATA_PUBLICACAO) <= $P{anoFim})
       )
   AND T.ID_DEPARTAMENTO=$P{unidade}
 ORDER BY T.TITULO,ANO]]></queryString>

