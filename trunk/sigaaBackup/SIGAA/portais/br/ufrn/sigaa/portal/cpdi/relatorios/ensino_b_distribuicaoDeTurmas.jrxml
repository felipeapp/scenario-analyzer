	<queryString><![CDATA[SELECT A.*,A.ate05_1+A.de06a10_1+A.de11a20_1+A.de21a30_1+A.de31a40_1+A.acimade40_1 AS TOTALGERAL
 FROM (
SELECT DISTINCT U.NOME AS UNIDADE, CC.ID_UNIDADE,T.ANO, T.PERIODO,CCD.NOME AS COMPONENTE,T.ID_DISCIPLINA,CC.CODIGO,
                (SELECT COUNT(ID_DISCIPLINA) 
                   FROM (
		                 SELECT T2.ID_DISCIPLINA,T2.ID_TURMA,COUNT(ID_DISCENTE) AS QTD
		                   FROM ENSINO.MATRICULA_COMPONENTE MC2
		                  INNER JOIN ENSINO.TURMA T2 ON T2.ID_TURMA=MC2.ID_TURMA
		                  WHERE T2.ANO=T.ANO and T2.PERIODO = T.PERIODO
                            AND T2.ID_DISCIPLINA=T.ID_DISCIPLINA
			    AND MC2.id_situacao_matricula != 11 -- Indeferida
                            AND T2.ID_SITUACAO_TURMA NOT IN (4,5)  -- Excluida, Cancelada
		                  GROUP BY T2.ID_DISCIPLINA,T2.ID_TURMA 
		                        HAVING COUNT(ID_DISCENTE)<=5
                        ) A1       
                ) AS ATE05_1,
                (SELECT COUNT(ID_DISCIPLINA) 
                   FROM (
		                 SELECT T2.ID_DISCIPLINA,T2.ID_TURMA,COUNT(ID_DISCENTE) AS QTD
		                   FROM ENSINO.MATRICULA_COMPONENTE MC2
		                  INNER JOIN ENSINO.TURMA T2 ON T2.ID_TURMA=MC2.ID_TURMA
		                  WHERE T2.ANO=T.ANO and T2.PERIODO = T.PERIODO
                            AND T2.ID_DISCIPLINA=T.ID_DISCIPLINA
			    AND MC2.id_situacao_matricula != 11 -- Indeferida
                            AND T2.ID_SITUACAO_TURMA NOT IN (4,5)  -- Excluida, Cancelada
		                  GROUP BY T2.ID_DISCIPLINA,T2.ID_TURMA 
		                        HAVING COUNT(ID_DISCENTE)>=6
                                   AND COUNT(ID_DISCENTE)<=10
                        ) A1       
                ) AS DE06A10_1,
                (SELECT COUNT(ID_DISCIPLINA) 
                   FROM (
		                 SELECT T2.ID_DISCIPLINA,T2.ID_TURMA,COUNT(ID_DISCENTE) AS QTD
		                   FROM ENSINO.MATRICULA_COMPONENTE MC2
		                  INNER JOIN ENSINO.TURMA T2 ON T2.ID_TURMA=MC2.ID_TURMA
		                  WHERE T2.ANO=T.ANO and T2.PERIODO = T.PERIODO
                            AND T2.ID_DISCIPLINA=T.ID_DISCIPLINA
			    AND MC2.id_situacao_matricula != 11 -- Indeferida
                            AND T2.ID_SITUACAO_TURMA NOT IN (4,5)  -- Excluida, Cancelada
		                  GROUP BY T2.ID_DISCIPLINA,T2.ID_TURMA 
		                        HAVING COUNT(ID_DISCENTE)>=11
                                   AND COUNT(ID_DISCENTE)<=20
                        ) A1       
                ) AS DE11A20_1,
                (SELECT COUNT(ID_DISCIPLINA) 
                   FROM (
		                 SELECT T2.ID_DISCIPLINA,T2.ID_TURMA,COUNT(ID_DISCENTE) AS QTD
		                   FROM ENSINO.MATRICULA_COMPONENTE MC2
		                  INNER JOIN ENSINO.TURMA T2 ON T2.ID_TURMA=MC2.ID_TURMA
		                  WHERE T2.ANO=T.ANO and T2.PERIODO = T.PERIODO
                            AND T2.ID_DISCIPLINA=T.ID_DISCIPLINA
			    AND MC2.id_situacao_matricula != 11 -- Indeferida
                            AND T2.ID_SITUACAO_TURMA NOT IN (4,5)  -- Excluida, Cancelada
		                  GROUP BY T2.ID_DISCIPLINA,T2.ID_TURMA 
		                        HAVING COUNT(ID_DISCENTE)>=21
                                   AND COUNT(ID_DISCENTE)<=30
                        ) A1       
                ) AS DE21A30_1,
                (SELECT COUNT(ID_DISCIPLINA) 
                   FROM (
		                 SELECT T2.ID_DISCIPLINA,T2.ID_TURMA,COUNT(ID_DISCENTE) AS QTD
		                   FROM ENSINO.MATRICULA_COMPONENTE MC2
		                  INNER JOIN ENSINO.TURMA T2 ON T2.ID_TURMA=MC2.ID_TURMA
		                  WHERE T2.ANO=T.ANO and T2.PERIODO = T.PERIODO
                            AND T2.ID_DISCIPLINA=T.ID_DISCIPLINA
			    AND MC2.id_situacao_matricula != 11 -- Indeferida
                            AND T2.ID_SITUACAO_TURMA NOT IN (4,5)  -- Excluida, Cancelada
		                  GROUP BY T2.ID_DISCIPLINA,T2.ID_TURMA 
		                        HAVING COUNT(ID_DISCENTE)>=31
                                   AND COUNT(ID_DISCENTE)<=40
                        ) A1       
                ) AS DE31A40_1,
                (SELECT COUNT(ID_DISCIPLINA) 
                   FROM (
		                 SELECT T2.ID_DISCIPLINA,T2.ID_TURMA,COUNT(ID_DISCENTE) AS QTD
		                   FROM ENSINO.MATRICULA_COMPONENTE MC2
		                  INNER JOIN ENSINO.TURMA T2 ON T2.ID_TURMA=MC2.ID_TURMA
		                  WHERE T2.ANO=T.ANO and T2.PERIODO = T.PERIODO
                            AND T2.ID_DISCIPLINA=T.ID_DISCIPLINA
			    AND MC2.id_situacao_matricula != 11 -- Indeferida
                            AND T2.ID_SITUACAO_TURMA NOT IN (4,5)  -- Excluida, Cancelada
		                  GROUP BY T2.ID_DISCIPLINA,T2.ID_TURMA 
		                        HAVING COUNT(ID_DISCENTE)>40
                        ) A1       
                ) AS ACIMADE40_1
 FROM ENSINO.TURMA T
 INNER JOIN ENSINO.COMPONENTE_CURRICULAR           CC ON CC.ID_DISCIPLINA=T.ID_DISCIPLINA
 INNER JOIN COMUM.UNIDADE U USING (ID_UNIDADE)
 INNER JOIN ENSINO.COMPONENTE_CURRICULAR_DETALHES CCD ON CCD.ID_COMPONENTE_DETALHES=CC.ID_DETALHE
 WHERE T.ANO=$P{ano} and (T.PERIODO = $P{periodo} OR $P{periodo} = -1)
    AND CC.ID_UNIDADE=$P{unidade}
   AND T.ID_SITUACAO_TURMA NOT IN (4,5) -- Excluida, Cancelada
 ORDER BY CC.ID_UNIDADE,T.ANO, T.PERIODO,CCD.NOME,T.ID_DISCIPLINA,CC.CODIGO
) A]]></queryString>

