	<queryString><![CDATA[select
  gest.nome as centro,
  und.nome as departamento,
  art.descricao as tema,
  prj.titulo as nome_projeto,
  pes.nome as membro,
  fmb.descricao as funcao
from
  extensao.atividade as atv
inner join
  extensao.area_tematica as art
  on atv.id_area_tematica_principal = art.id_area_tematica
inner join
  projetos.projeto as prj
  on atv.id_atividade = prj.id_projeto
inner join
  projetos.membro_projeto as mpj
  on atv.id_atividade = mpj.id_atividade
inner join
  projetos.funcao_membro as fmb
  on mpj.id_funcao_membro = fmb.id_funcao_membro
inner join
  rh.servidor as sev
  on mpj.id_servidor = sev.id_servidor
inner join
  comum.pessoa as pes
  on pes.id_pessoa = sev.id_pessoa
inner join
  comum.unidade as und
  on prj.id_unidade = und.id_unidade
inner join
  comum.unidade as gest
  on und.id_gestora = gest.id_unidade
where
  atv.id_tipo_atividade_extensao = $P{tipoAcao} 
and 
  prj.id_tipo_situacao_projeto = $P{situacaoAcao} 
and 
  sev.id_categoria = $P{tipoServidor} 
and 
  (case $P{funcaoMembro} when 0 then 
    true 
  else
    mpj.id_funcao_membro = $P{funcaoMembro}
  end)
and 
  (extract(year from prj.data_inicio) <= $P{ano} and extract(year from prj.data_fim) >= $P{ano})
order by
  gest.nome, und.nome, art.descricao asc]]></queryString>

