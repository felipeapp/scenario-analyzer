	<queryString><![CDATA[--8041
SELECT * FROM (
SELECT DISTINCT /* C.NOME AS CURSO,COALESCE(ATE.DESCRICAO,'') AS ESPECIALIZACAO, */
       /*ECT.PRAZO_MAX_CONCLUSAO,*/C.NOME AS CURSO,P2.NOME AS ORIENTADOR_NOME,D.MATRICULA,P.NOME AS DISCENTE_NOME/*,
       D.STATUS*/,SD.DESCRICAO AS DESCRICAO_STATUS
  FROM PUBLIC.DISCENTE D
  INNER JOIN TECNICO.DISCENTE_TECNICO DT ON DT.ID_DISCENTE=D.ID_DISCENTE
  INNER JOIN GRADUACAO.ORIENTACAO_ACADEMICA O ON O.ID_DISCENTE=D.ID_DISCENTE
  LEFT  JOIN TECNICO.TURMA_ENTRADA_TECNICO TE ON TE.ID_TURMA_ENTRADA=DT.ID_TURMA_ENTRADA
--  LEFT  JOIN TECNICO.ESPECIALIZACAO_TURMA_ENTRADA ATE ON ATE.ID_ESPECIALIZACAO_TURMA_ENTRADA=TE.ID_ESPECIALIZACAO_TURMA_ENTRADA
  INNER JOIN PUBLIC.CURSO C ON C.ID_CURSO=D.ID_CURSO
  INNER JOIN COMUM.PESSOA P ON P.ID_PESSOA=D.ID_PESSOA
  INNER JOIN RH.SERVIDOR S ON S.ID_SERVIDOR=O.ID_SERVIDOR
  INNER JOIN COMUM.PESSOA P2 ON P2.ID_PESSOA=S.ID_PESSOA
--  INNER JOIN TECNICO.ESTRUTURA_CURRICULAR_TECNICA ECT ON ECT.ID_CURSO=D.ID_CURSO
  INNER JOIN PUBLIC.STATUS_DISCENTE SD ON SD.STATUS=D.STATUS
WHERE C.ID_UNIDADE=$P{unidade} -- 284 = MUSICA
  AND C.NIVEL='T'
  AND TE.ANO_REFERENCIA=$P{ano}
  AND (S.ID_SERVIDOR=$P{servidor} OR $P{servidor}=-1)
) A  
ORDER BY ORIENTADOR_NOME,DESCRICAO_STATUS,DISCENTE_NOME]]></queryString>

