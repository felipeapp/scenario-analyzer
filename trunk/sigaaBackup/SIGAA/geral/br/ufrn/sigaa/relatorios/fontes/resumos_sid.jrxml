	<queryString><![CDATA[select distinct u.id_unidade, u.nome as nome_centro, r.id_resumo_sid, r.resumo, r.ano_sid, r.palavras_chave, 
	r.data_envio, r.id_status, status.descricao, pro.titulo, p_coord.nome as nome_coord
from monitoria.resumo_sid as r
inner join monitoria.status_relatorio as status on r.id_status = status.id
inner join monitoria.projeto_monitoria as pro_ens on pro_ens.id_projeto_monitoria = r.id_projeto_monitoria 
inner join projetos.projeto as pro on pro.id_projeto = pro_ens.id_projeto 
inner join comum.unidade as u on u.id_unidade = pro.id_unidade 
inner join projetos.membro_projeto as coord on coord.id_membro_projeto = pro.id_coordenador 
inner join comum.pessoa as p_coord on p_coord.id_pessoa = coord.id_pessoa 
where r.ano_sid = $P!{ano} and r.id_status = 3 and r.ativo = true and pro.id_tipo_projeto = 1 
order by u.id_unidade, r.id_resumo_sid asc]]></queryString>

