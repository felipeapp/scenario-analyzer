	<queryString><![CDATA[SELECT A.ACAO,
     (SELECT 'Número de livros' FROM extensao.area_tematica LIMIT 1) AS INDICADOR,
     (SELECT count(*) FROM 
        (SELECT id_area_tematica_principal 
                  FROM extensao.atividade a
                 INNER JOIN projetos.projeto p on p.id_projeto=a.id_atividade
                 INNER JOIN extensao.produto c on c.id_produto=a.id_produto
                 WHERE a.id_area_tematica_principal=1 -- Comunicacao
                   AND p.ano BETWEEN $P{anoInicio} AND $P{anoFim}
                   AND p.id_tipo_situacao_projeto in (103,105,110) -- aprovado/concluido/registro aprovado
                   AND a.id_produto is not NULL 
                   AND c.id_tipo_produto=7
         ) s1
      )  AS comunicacao,
     (SELECT count(*) FROM 
        (SELECT id_area_tematica_principal 
                  FROM extensao.atividade a
                 INNER JOIN projetos.projeto p on p.id_projeto=a.id_atividade
                 INNER JOIN extensao.produto c on c.id_produto=a.id_produto
                 WHERE a.id_area_tematica_principal=2 -- Cultura
                   AND p.ano BETWEEN $P{anoInicio} AND $P{anoFim}
                   AND p.id_tipo_situacao_projeto in (103,105,110) -- aprovado/concluido/registro aprovado
                   AND a.id_produto is not NULL 
                   AND c.id_tipo_produto=7
         ) s1
      )  AS cultura,
     (SELECT count(*) FROM 
        (SELECT id_area_tematica_principal 
                  FROM extensao.atividade a
                 INNER JOIN projetos.projeto p on p.id_projeto=a.id_atividade
                 INNER JOIN extensao.produto c on c.id_produto=a.id_produto
                 WHERE a.id_area_tematica_principal=3 -- desenvolvimento_rural
                   AND p.ano BETWEEN $P{anoInicio} AND $P{anoFim}
                   AND p.id_tipo_situacao_projeto in (103,105,110) -- aprovado/concluido/registro aprovado
                   AND a.id_produto is not NULL 
                   AND c.id_tipo_produto=7
         ) s1
      )  AS desenvolvimento_rural,
     (SELECT count(*) FROM 
        (SELECT id_area_tematica_principal 
                  FROM extensao.atividade a
                 INNER JOIN projetos.projeto p on p.id_projeto=a.id_atividade
                 INNER JOIN extensao.produto c on c.id_produto=a.id_produto
                 WHERE a.id_area_tematica_principal=4 -- Direitos Humanos
                   AND p.ano BETWEEN $P{anoInicio} AND $P{anoFim}
                   AND p.id_tipo_situacao_projeto in (103,105,110) -- aprovado/concluido/registro aprovado
                   AND a.id_produto is not NULL 
                   AND c.id_tipo_produto=7
         ) s1
      )  AS direitos_humanos,
     (SELECT count(*) FROM 
        (SELECT id_area_tematica_principal 
                  FROM extensao.atividade a
                 INNER JOIN projetos.projeto p on p.id_projeto=a.id_atividade
                 INNER JOIN extensao.produto c on c.id_produto=a.id_produto
                 WHERE a.id_area_tematica_principal=5 -- educacao
                   AND p.ano BETWEEN $P{anoInicio} AND $P{anoFim}
                   AND p.id_tipo_situacao_projeto in (103,105,110) -- aprovado/concluido/registro aprovado
                   AND a.id_produto is not NULL 
                   AND c.id_tipo_produto=7
         ) s1
      )  AS educacao,
     (SELECT count(*) FROM 
        (SELECT id_area_tematica_principal 
                  FROM extensao.atividade a
                 INNER JOIN projetos.projeto p on p.id_projeto=a.id_atividade
                 INNER JOIN extensao.produto c on c.id_produto=a.id_produto
                 WHERE a.id_area_tematica_principal=6 -- meio_ambiente
                   AND p.ano BETWEEN $P{anoInicio} AND $P{anoFim}
                   AND p.id_tipo_situacao_projeto in (103,105,110) -- aprovado/concluido/registro aprovado
                   AND a.id_produto is not NULL 
                   AND c.id_tipo_produto=7
         ) s1
      )  AS meio_ambiente,
     (SELECT count(*) FROM 
        (SELECT id_area_tematica_principal 
                  FROM extensao.atividade a
                 INNER JOIN projetos.projeto p on p.id_projeto=a.id_atividade
                 INNER JOIN extensao.produto c on c.id_produto=a.id_produto
                 WHERE a.id_area_tematica_principal=7 -- saude
                   AND p.ano BETWEEN $P{anoInicio} AND $P{anoFim}
                   AND p.id_tipo_situacao_projeto in (103,105,110) -- aprovado/concluido/registro aprovado
                   AND a.id_produto is not NULL 
                   AND c.id_tipo_produto=7
         ) s1
      )  AS saude,
     (SELECT count(*) FROM 
        (SELECT id_area_tematica_principal 
                  FROM extensao.atividade a
                 INNER JOIN projetos.projeto p on p.id_projeto=a.id_atividade
                 INNER JOIN extensao.produto c on c.id_produto=a.id_produto
                 WHERE a.id_area_tematica_principal=8 -- Tecnologia
                   AND p.ano BETWEEN $P{anoInicio} AND $P{anoFim}
                   AND p.id_tipo_situacao_projeto in (103,105,110) -- aprovado/concluido/registro aprovado
                   AND a.id_produto is not NULL 
                   AND c.id_tipo_produto=7
         ) s1
      )  AS tecnologia,
     (SELECT count(*) FROM 
        (SELECT id_area_tematica_principal 
                  FROM extensao.atividade a
                 INNER JOIN projetos.projeto p on p.id_projeto=a.id_atividade
                 INNER JOIN extensao.produto c on c.id_produto=a.id_produto
                 WHERE a.id_area_tematica_principal=9 -- trabalho
                   AND p.ano BETWEEN $P{anoInicio} AND $P{anoFim}
                   AND p.id_tipo_situacao_projeto in (103,105,110) -- aprovado/concluido/registro aprovado
                   AND a.id_produto is not NULL 
                   AND c.id_tipo_produto=7
         ) s1
      )  AS trabalho,
     (SELECT count(*) FROM 
        (SELECT id_area_tematica_principal 
                  FROM extensao.atividade a
                 INNER JOIN projetos.projeto p on p.id_projeto=a.id_atividade
                 INNER JOIN extensao.produto c on c.id_produto=a.id_produto
                   AND p.ano BETWEEN $P{anoInicio} AND $P{anoFim}
                   AND p.id_tipo_situacao_projeto in (103,105,110) -- aprovado/concluido/registro aprovado
                   AND a.id_produto is not NULL 
                   AND c.id_tipo_produto=7
         ) s1
      )  AS total
 FROM (SELECT 'PUBLICAÇÕES' AS ACAO FROM extensao.area_tematica) A LIMIT 1]]></queryString>

