	<queryString><![CDATA[SELECT U.NOME AS DEPARTAMENTO,P.NOME,CC.CODIGO AS DISCIPLINA,CCD.NOME AS NOME_DISCIPLINA,
       T.CODIGO AS TURMA, (SELECT COUNT(*) FROM ENSINO.MATRICULA_COMPONENTE MC WHERE MC.ID_TURMA = T.ID_TURMA) as TOTAL_MATRICULADOS
  FROM ENSINO.TURMA T
 INNER JOIN ENSINO.DOCENTE_TURMA D ON D.ID_TURMA=T.ID_TURMA
 INNER JOIN ENSINO.COMPONENTE_CURRICULAR CC ON CC.ID_DISCIPLINA=T.ID_DISCIPLINA
 INNER JOIN ENSINO.COMPONENTE_CURRICULAR_DETALHES CCD ON CCD.ID_COMPONENTE=T.ID_DISCIPLINA
 INNER JOIN RH.SERVIDOR S   ON S.ID_SERVIDOR=D.ID_DOCENTE
 INNER JOIN COMUM.PESSOA P ON P.ID_PESSOA=S.ID_PESSOA
 INNER JOIN COMUM.UNIDADE U ON U.ID_UNIDADE=S.ID_UNIDADE
 WHERE T.ano=$P{ano}
   AND T.PERIODO=$P{periodo}
   AND (($P{unidade}=-1) OR (S.ID_UNIDADE=$P{unidade}))
   AND T.ID_SITUACAO_TURMA=1 -- ABERTA
 ORDER BY U.NOME,P.NOME,CCD.NOME]]></queryString>

