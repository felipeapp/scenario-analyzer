	<parameter name="id_resumo" isForPrompting="false" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(-1)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select distinct disc.matricula, p_disc.nome as nome_disc
from monitoria.participacao_sid as part_sid 
inner join monitoria.discente_monitoria as dis_moni on dis_moni.id_discente_monitoria = part_sid.id_discente_monitoria 
inner join graduacao.discente_graduacao as dis_gra on dis_gra.id_discente_graduacao = dis_moni.id_discente 
inner join public.discente as disc on disc.id_discente = dis_gra.id_discente_graduacao 
inner join comum.pessoa as p_disc on p_disc.id_pessoa = disc.id_pessoa 
where part_sid.id_resumo_sid = $P!{id_resumo} and part_sid.ativo = trueValue() and part_sid.participou = trueValue() 
order by p_disc.nome asc]]></queryString>

