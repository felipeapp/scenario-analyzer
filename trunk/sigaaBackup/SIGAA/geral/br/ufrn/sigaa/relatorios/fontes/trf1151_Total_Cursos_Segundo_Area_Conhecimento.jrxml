	<queryString><![CDATA[select 'até 30 Horas' as grupo_total_horas, nome, ministrantes.total_cursos,
  totais.total_carga_horaria, totais.total_concluintes, 
  ministrantes.total_docentes, ministrantes.total_tecnicos, ministrantes.total_discentes, ministrantes.total_externos ,
  id_area_conhecimento_cnpq
  from comum.area_conhecimento_cnpq area 	
  left join (select ac.id_grande_area as id_area,
	count(distinct c.id_curso_evento) as total_cursos,
	count(distinct docentes.id_servidor) as total_docentes,
	count(distinct tecnicos.id_servidor) as total_tecnicos,
	count(distinct discentes.id_discente) as total_discentes,
	count(distinct mp.id_participante_externo) as total_externos
	from comum.area_conhecimento_cnpq ac
	join extensao.atividade a on(a.id_area_conhecimento_cnpq = ac.id_area_conhecimento_cnpq and a.id_tipo_atividade_extensao = 3) 
	join projetos.projeto proj on(proj.id_projeto = a.id_atividade  
		and proj.id_tipo_situacao_projeto in (103,105)
		and (extract(year from proj.data_inicio) <= $P{ano} and extract(year from proj.data_fim) >= $P{ano}) )
	join extensao.curso_evento c on (a.id_curso_evento = c.id_curso_evento and c.carga_horaria <= 30) -- Fazer outra versão para ch > 30
	left join projetos.membro_projeto mp on(mp.id_atividade = a.id_atividade and mp.ativo is true)
	left join rh.servidor docentes on(mp.id_servidor = docentes.id_servidor and docentes.id_categoria = 1)
	left join rh.servidor tecnicos on(mp.id_servidor = tecnicos.id_servidor and tecnicos.id_categoria = 2)
	left join discente discentes on(mp.id_discente = discentes.id_discente)
	where not exists (select id_atividade_filha from extensao.atividade_atividade where id_atividade_filha = a.id_atividade)
	group by ac.id_grande_area) 
	as ministrantes on (area.id_area_conhecimento_cnpq = ministrantes.id_area)
  left join (select ac.id_grande_area as id_area,
	coalesce(sum(c.carga_horaria),0) as total_carga_horaria,
	-- coalesce(sum(c.numero_participantes),0) as total_participantes,
	coalesce(sum(a.publico_atendido),0) as total_concluintes
	from comum.area_conhecimento_cnpq ac
	join extensao.atividade a on(a.id_area_conhecimento_cnpq = ac.id_area_conhecimento_cnpq and a.id_tipo_atividade_extensao = 3) 
	join extensao.curso_evento c on (a.id_curso_evento = c.id_curso_evento and c.carga_horaria <= 30)
	join projetos.projeto proj on(proj.id_projeto = a.id_atividade  
		and proj.id_tipo_situacao_projeto in (103,105) 
		and (extract(year from proj.data_inicio) <= $P{ano} and extract(year from proj.data_fim) >= $P{ano}) )
	where not exists (select id_atividade_filha from extensao.atividade_atividade where id_atividade_filha = a.id_atividade)
	group by ac.id_grande_area) 
	as totais on (area.id_area_conhecimento_cnpq = totais.id_area)
where area.id_grande_area = area.id_area_conhecimento_cnpq and area.excluido = false
union -- mais de trinta
select 'Acima de 30 Horas' as grupo_total_horas, nome, ministrantes.total_cursos,
  totais.total_carga_horaria, totais.total_concluintes, 
  ministrantes.total_docentes, ministrantes.total_tecnicos, ministrantes.total_discentes, ministrantes.total_externos ,
  id_area_conhecimento_cnpq 
  from comum.area_conhecimento_cnpq area 	
  left join (select ac.id_grande_area as id_area,
	count(distinct c.id_curso_evento) as total_cursos,
	count(distinct docentes.id_servidor) as total_docentes,
	count(distinct tecnicos.id_servidor) as total_tecnicos,
	count(distinct discentes.id_discente) as total_discentes,
	count(distinct mp.id_participante_externo) as total_externos
	from comum.area_conhecimento_cnpq ac
	join extensao.atividade a on(a.id_area_conhecimento_cnpq = ac.id_area_conhecimento_cnpq and a.id_tipo_atividade_extensao = 3) 
	join projetos.projeto proj on(proj.id_projeto = a.id_atividade  
		and proj.id_tipo_situacao_projeto in (103,105)
		and (extract(year from proj.data_inicio) <= $P{ano} and extract(year from proj.data_fim) >= $P{ano}) )
	join extensao.curso_evento c on (a.id_curso_evento = c.id_curso_evento and c.carga_horaria > 30)
	left join projetos.membro_projeto mp on(mp.id_atividade = a.id_atividade and mp.ativo is true)
	left join rh.servidor docentes on(mp.id_servidor = docentes.id_servidor and docentes.id_categoria = 1)
	left join rh.servidor tecnicos on(mp.id_servidor = tecnicos.id_servidor and tecnicos.id_categoria = 2)
	left join discente discentes on(mp.id_discente = discentes.id_discente)
	where not exists (select id_atividade_filha from extensao.atividade_atividade where id_atividade_filha = a.id_atividade)
	group by ac.id_grande_area) 
	as ministrantes on (area.id_area_conhecimento_cnpq = ministrantes.id_area)
  left join (select ac.id_grande_area as id_area,
	coalesce(sum(c.carga_horaria),0) as total_carga_horaria,
	--coalesce(sum(c.numero_participantes),0) as total_participantes,
	coalesce(sum(a.publico_atendido),0) as total_concluintes
	from comum.area_conhecimento_cnpq ac
	join extensao.atividade a on(a.id_area_conhecimento_cnpq = ac.id_area_conhecimento_cnpq and a.id_tipo_atividade_extensao = 3) 
	join extensao.curso_evento c on (a.id_curso_evento = c.id_curso_evento and c.carga_horaria > 30)
	join projetos.projeto proj on(proj.id_projeto = a.id_atividade  
		and proj.id_tipo_situacao_projeto in (103,105) 
		and (extract(year from proj.data_inicio) <= $P{ano} and extract(year from proj.data_fim) >= $P{ano}) )
	where not exists (select id_atividade_filha from extensao.atividade_atividade where id_atividade_filha = a.id_atividade)
	group by ac.id_grande_area) 
	as totais on (area.id_area_conhecimento_cnpq = totais.id_area)
where area.id_grande_area = area.id_area_conhecimento_cnpq and area.excluido = false

order by grupo_total_horas desc, id_area_conhecimento_cnpq]]></queryString>

