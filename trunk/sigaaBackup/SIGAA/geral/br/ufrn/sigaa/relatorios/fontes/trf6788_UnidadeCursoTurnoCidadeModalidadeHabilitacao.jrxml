	<queryString><![CDATA[SELECT DISTINCT U.SIGLA AS CENTRO,C.NOME AS CURSO,
       E.NOME AS ENFASE,
       G.DESCRICAO AS MODALIDADE,
       H.NOME AS HABILITACAO,
       T.SIGLA AS TURNO,
       M.NOME AS MUNICIPIO
  FROM PUBLIC.CURSO C 
 INNER JOIN COMUM.UNIDADE U               ON U.ID_UNIDADE=C.ID_UNIDADE
 INNER JOIN COMUM.MODALIDADE_EDUCACAO ME  ON ME.ID_MODALIDADE_EDUCACAO=C.ID_MODALIDADE_EDUCACAO
 INNER JOIN GRADUACAO.MATRIZ_CURRICULAR MC ON MC.ID_CURSO=C.ID_CURSO
  LEFT JOIN ENSINO.TURNO T				   ON T.ID_TURNO=MC.ID_TURNO
  LEFT JOIN GRADUACAO.HABILITACAO H		   ON H.ID_HABILITACAO=MC.ID_HABILITACAO
  LEFT JOIN ENSINO.GRAU_ACADEMICO G		   ON G.ID_GRAU_ACADEMICO=MC.ID_GRAU_ACADEMICO
  LEFT JOIN COMUM.MUNICIPIO M             ON C.ID_MUNICIPIO=M.ID_MUNICIPIO
  LEFT JOIN GRADUACAO.ENFASE E            ON E.ID_ENFASE=MC.ID_ENFASE
  WHERE C.NIVEL='G' AND MC.ATIVO='TRUE'
  ORDER BY U.SIGLA,C.NOME,MUNICIPIO,T.SIGLA]]></queryString>

