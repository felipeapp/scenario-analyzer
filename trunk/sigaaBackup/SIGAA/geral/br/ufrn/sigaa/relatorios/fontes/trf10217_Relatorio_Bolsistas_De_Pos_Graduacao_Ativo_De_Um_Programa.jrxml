	<queryString><![CDATA[select 1 AS CONTA,
a.matricula,
lpad(cast(p.cpf_cnpj as text), 11, '0')as cpf_cnpj, 
p.nome as nome_aluno, 
tb.denominacao as tipo_bolsa, 
bo.inicio, 
bo.fim, 
c.id_unidade, 
u.nome as nome_programa,
u2.sigla as convenio
from academico.aluno a
left join comum.pessoa p on p.id_pessoa = a.id_pessoa
left join academico.curso c on c.id_curso = a.id_curso
inner join bolsas.bolsista b on b.id_aluno = a.id_aluno
inner join bolsas.bolsa bo on bo.id_bolsista = b.id
left join bolsas.tipo_bolsa tb on tb.id = bo.id_tipo_bolsa
left join comum.unidade u on c.id_unidade = u.id_unidade
left join contratos.convenio conv on conv.id = bo.id_convenio
left join comum.unidade u2 on conv.id_unidade_convenio = u2.id_unidade
where c.nivel IN ('E', 'D')
and (bo.fim >= now() or bo.fim is null)
and (($P{unidadeID}=-1) or (c.id_unidade = $P{unidadeID})) --Programa
and a.id_status = 1 -- ativo
and bo.finalizada is false
order by u.nome, p.nome]]></queryString>

