	<queryString><![CDATA[select h.id_horario, ( to_char(h.inicio, 'HH24:MI') || ' - ' ||  to_char(h.fim, 'HH24:MI') ) as horario, h.tipo, h.ordem,
(
	select count(t2.id_turma)
	from ensino.horario h2 inner join ensino.horario_turma ht2 on ht2.id_horario = h2.id_horario
	inner join ensino.turma t2 on t2.id_turma = ht2.id_turma
	inner join ensino.componente_curricular cc2 on cc2.id_disciplina = t2.id_disciplina
	inner join COMUM.UNIDADE u2 on u2.id_unidade = cc2.id_unidade
	where h2.nivel = 'G'
	and (($P{centro}=-1) or (u2.unidade_responsavel = $P{centro})) --Centro
	and (($P{departamento}=-1) or (cc2.id_unidade = $P{departamento})) --Departamento
	and (($P{ano}=-1) or (t2.ano = $P{ano})) --Ano
	and (($P{periodo}=-1) or (t2.periodo = $P{periodo})) --Periodo
	and ht2.id_horario = h.id_horario
	and ht2.dia = '2'
	group by h2.id_horario, h2.inicio, h2.fim, h2.tipo, h2.ordem
) as qtd_segunda,

/*
(select count(id_turma) from ensino.horario_turma ht2 where ht2.id_horario = h.id_horario and dia = '3') as qtd_terca,
(select count(id_turma) from ensino.horario_turma ht2 where ht2.id_horario = h.id_horario and dia = '4') as qtd_quarta,
(select count(id_turma) from ensino.horario_turma ht2 where ht2.id_horario = h.id_horario and dia = '5') as qtd_quinta,
(select count(id_turma) from ensino.horario_turma ht2 where ht2.id_horario = h.id_horario and dia = '6') as qtd_sexta,
(select count(id_turma) from ensino.horario_turma ht2 where ht2.id_horario = h.id_horario and dia = '7') as qtd_sabado,
(select count(id_turma) from ensino.horario_turma ht2 where ht2.id_horario = h.id_horario) as qtd_geral
*/

(
	select count(t2.id_turma)
	from ensino.horario h2 inner join ensino.horario_turma ht2 on ht2.id_horario = h2.id_horario
	inner join ensino.turma t2 on t2.id_turma = ht2.id_turma
	inner join ensino.componente_curricular cc2 on cc2.id_disciplina = t2.id_disciplina
	inner join COMUM.UNIDADE u2 on u2.id_unidade = cc2.id_unidade
	where h2.nivel = 'G'
	and (($P{centro}=-1) or (u2.unidade_responsavel = $P{centro})) --Centro
	and (($P{departamento}=-1) or (cc2.id_unidade = $P{departamento})) --Departamento
	and (($P{ano}=-1) or (t2.ano = $P{ano})) --Ano
	and (($P{periodo}=-1) or (t2.periodo = $P{periodo})) --Periodo
	and ht2.id_horario = h.id_horario
	and ht2.dia = '3'
	group by h2.id_horario, h2.inicio, h2.fim, h2.tipo, h2.ordem
) as qtd_terca,

(
	select count(t2.id_turma)
	from ensino.horario h2 inner join ensino.horario_turma ht2 on ht2.id_horario = h2.id_horario
	inner join ensino.turma t2 on t2.id_turma = ht2.id_turma
	inner join ensino.componente_curricular cc2 on cc2.id_disciplina = t2.id_disciplina
	inner join COMUM.UNIDADE u2 on u2.id_unidade = cc2.id_unidade
	where h2.nivel = 'G'
	and (($P{centro}=-1) or (u2.unidade_responsavel = $P{centro})) --Centro
	and (($P{departamento}=-1) or (cc2.id_unidade = $P{departamento})) --Departamento
	and (($P{ano}=-1) or (t2.ano = $P{ano})) --Ano
	and (($P{periodo}=-1) or (t2.periodo = $P{periodo})) --Periodo
	and ht2.id_horario = h.id_horario
	and ht2.dia = '4'
	group by h2.id_horario, h2.inicio, h2.fim, h2.tipo, h2.ordem
) as qtd_quarta,

(
	select count(t2.id_turma)
	from ensino.horario h2 inner join ensino.horario_turma ht2 on ht2.id_horario = h2.id_horario
	inner join ensino.turma t2 on t2.id_turma = ht2.id_turma
	inner join ensino.componente_curricular cc2 on cc2.id_disciplina = t2.id_disciplina
	inner join COMUM.UNIDADE u2 on u2.id_unidade = cc2.id_unidade
	where h2.nivel = 'G'
	and (($P{centro}=-1) or (u2.unidade_responsavel = $P{centro})) --Centro
	and (($P{departamento}=-1) or (cc2.id_unidade = $P{departamento})) --Departamento
	and (($P{ano}=-1) or (t2.ano = $P{ano})) --Ano
	and (($P{periodo}=-1) or (t2.periodo = $P{periodo})) --Periodo
	and ht2.id_horario = h.id_horario
	and ht2.dia = '5'
	group by h2.id_horario, h2.inicio, h2.fim, h2.tipo, h2.ordem
) as qtd_quinta,

(
	select count(t2.id_turma)
	from ensino.horario h2 inner join ensino.horario_turma ht2 on ht2.id_horario = h2.id_horario
	inner join ensino.turma t2 on t2.id_turma = ht2.id_turma
	inner join ensino.componente_curricular cc2 on cc2.id_disciplina = t2.id_disciplina
	inner join COMUM.UNIDADE u2 on u2.id_unidade = cc2.id_unidade
	where h2.nivel = 'G'
	and (($P{centro}=-1) or (u2.unidade_responsavel = $P{centro})) --Centro
	and (($P{departamento}=-1) or (cc2.id_unidade = $P{departamento})) --Departamento
	and (($P{ano}=-1) or (t2.ano = $P{ano})) --Ano
	and (($P{periodo}=-1) or (t2.periodo = $P{periodo})) --Periodo
	and ht2.id_horario = h.id_horario
	and ht2.dia = '6'
	group by h2.id_horario, h2.inicio, h2.fim, h2.tipo, h2.ordem
) as qtd_sexta,

(
	select count(t2.id_turma)
	from ensino.horario h2 inner join ensino.horario_turma ht2 on ht2.id_horario = h2.id_horario
	inner join ensino.turma t2 on t2.id_turma = ht2.id_turma
	inner join ensino.componente_curricular cc2 on cc2.id_disciplina = t2.id_disciplina
	inner join COMUM.UNIDADE u2 on u2.id_unidade = cc2.id_unidade
	where h2.nivel = 'G'
	and (($P{centro}=-1) or (u2.unidade_responsavel = $P{centro})) --Centro
	and (($P{departamento}=-1) or (cc2.id_unidade = $P{departamento})) --Departamento
	and (($P{ano}=-1) or (t2.ano = $P{ano})) --Ano
	and (($P{periodo}=-1) or (t2.periodo = $P{periodo})) --Periodo
	and ht2.id_horario = h.id_horario
	and ht2.dia = '7'
	group by h2.id_horario, h2.inicio, h2.fim, h2.tipo, h2.ordem
) as qtd_sabado

from ensino.horario h inner join ensino.horario_turma ht on ht.id_horario = h.id_horario
inner join ensino.turma t on t.id_turma = ht.id_turma
inner join ensino.componente_curricular cc on cc.id_disciplina = t.id_disciplina
inner join COMUM.UNIDADE u on u.id_unidade = cc.id_unidade

where h.nivel = 'G'
and (($P{centro}=-1) or (u.unidade_responsavel = $P{centro})) --Centro
and (($P{departamento}=-1) or (cc.id_unidade = $P{departamento})) --Departamento
and (($P{ano}=-1) or (t.ano = $P{ano})) --Ano
and (($P{periodo}=-1) or (t.periodo = $P{periodo})) --Periodo

group by h.id_horario, h.inicio, h.fim, h.tipo, h.ordem
order by tipo, ordem]]></queryString>

