	<queryString><![CDATA[SELECT * 
  FROM (
        SELECT CASE C.NIVEL
                  WHEN 'D' THEN 'DOUTORADO'
                  WHEN 'E' THEN 'MESTRADO'
               END AS NIVEL,
               U2.ID_UNIDADE AS ID_CENTRO,U2.NOME AS CENTRO,C.ID_CURSO,C.NOME AS NOMECURSO,
               (SELECT CONCEITO 
                  FROM STRICTO_SENSU.RECOMENDACAO R
                  WHERE R.ID_CURSO=C.ID_CURSO
                  ORDER BY DATAAVALIACAO DESC
                  LIMIT 1) AS CONCEITO,
               (SELECT LPS.DENOMINACAO 
                  FROM STRICTO_SENSU.LINHA_PESQUISA_STRICTO LPS 
                 WHERE LPS.ID_PROGRAMA=U.ID_UNIDADE LIMIT 1) AS LINHA_PESQUISA,
               (SELECT AC.DENOMINACAO
                  FROM stricto_sensu.AREA_CONCENTRACAO AC  
                 WHERE AC.ID_PROGRAMA=U.ID_UNIDADE 
                   AND AC.NIVEL=C.NIVEL LIMIT 1) AS AREA_CONCENTRACAO
          FROM PUBLIC.CURSO C 
         INNER JOIN COMUM.UNIDADE U ON U.ID_UNIDADE=C.ID_UNIDADE
         INNER JOIN COMUM.UNIDADE U2 ON U2.ID_UNIDADE=U.UNIDADE_RESPONSAVEL
          WHERE ((U2.ID_UNIDADE=$P{centro}) OR ($P{centro}=-1))
            AND C.NIVEL IN ('D','E')
        ) A
WHERE CONCEITO IS NOT NULL        
 ORDER BY CENTRO,CONCEITO DESC]]></queryString>

