	<queryString><![CDATA[select 
idAtividade ,
atividade ,
sum(case when situacao = 1 then 1 else 0 end) as INSCRITO,
sum(case when situacao = 2 then 1 else 0 end) as PRESENTE
from (

	select 
	a.id_atividade_atualizacao_pedagogica as idAtividade , a.nome as atividade , p.situacao as situacao 
	from apedagogica.atividade_atualizacao_pedagogica a 
	left join apedagogica.participante_atividade_atualizacao_pedagogica p on a.id_atividade_atualizacao_pedagogica = p.id_atividade_atualizacao_pedagogica
	where inicio >= $P{dataInicial} and fim <= $P{dataFinal} and a.ativo = trueValue()

) as consulta	
group by idAtividade , atividade
order by atividade]]></queryString>

