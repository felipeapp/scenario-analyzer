	<parameter name="ano" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(2009)]]></defaultValueExpression>
	</parameter>
	<parameter name="situacaoAcao" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(103)]]></defaultValueExpression>
	</parameter>
	<parameter name="tipoAcao" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(2)]]></defaultValueExpression>
	</parameter>
	<parameter name="tipoServidor" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(2)]]></defaultValueExpression>
	</parameter>
	<parameter name="funcaoMembro" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(1)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select
  gest.nome as centro,
  count(gest.nome) as total
from
  extensao.atividade as atv
inner join
  projetos.projeto as prj
  on atv.id_atividade = prj.id_projeto
inner join
  projetos.membro_projeto as mpj
  on atv.id_atividade = mpj.id_atividade
inner join
  rh.servidor as sev
  on mpj.id_servidor = sev.id_servidor
inner join
  comum.pessoa as pes
  on pes.id_pessoa = sev.id_pessoa
inner join
  comum.unidade as und
  on prj.id_unidade = und.id_unidade
inner join
  comum.unidade as gest
  on und.id_gestora = gest.id_unidade
where
  atv.id_tipo_atividade_extensao = $P{tipoAcao} 
and 
  prj.id_tipo_situacao_projeto = $P{situacaoAcao} 
and 
  sev.id_categoria = $P{tipoServidor} 
and 
  (case $P{funcaoMembro} when 0 then 
    true 
  else
    mpj.id_funcao_membro = $P{funcaoMembro}
  end)
and 
  (extract(year from prj.data_inicio) <= $P{ano} and extract(year from prj.data_fim) >= $P{ano})
group by
  gest.nome]]></queryString>

	<field name="centro" class="java.lang.String"/>
	<field name="total" class="java.lang.Long"/>

