	<queryString><![CDATA[select at.descricao,
sum(CASE WHEN id_tipo_curso_evento in (11,12) THEN 1 ELSE 0 END) AS congresso,
sum(CASE WHEN id_tipo_curso_evento in (28) THEN 1 ELSE 0 END) AS seminario,
sum(CASE WHEN id_tipo_curso_evento in (6,7,13,27) THEN 1 ELSE 0 END) AS debate,
sum(CASE WHEN id_tipo_curso_evento in (16,17,21,23) THEN 1 ELSE 0 END) AS exposicao,
sum(CASE WHEN id_tipo_curso_evento in (9,15,26) THEN 1 ELSE 0 END) AS espetaculo,
sum(CASE WHEN id_tipo_curso_evento in (5,24,30) THEN 1 ELSE 0 END) AS esportivo,
sum(CASE WHEN id_tipo_curso_evento = 18 THEN 1 ELSE 0 END) AS festival,
sum(CASE WHEN id_tipo_curso_evento not in (11,12,28,6,7,13,27,16,17,21,23,9,15,26,5,24,30,18) THEN 1 ELSE 0 END) AS outros,
count(distinct e.id_curso_evento) AS total,
coalesce(sum(a.publico_atendido),0) as total_publico
from extensao.area_tematica at
left join extensao.atividade a on(a.id_area_tematica_principal = at.id_area_tematica and a.id_tipo_atividade_extensao = 4)
join extensao.curso_evento e on (a.id_curso_evento = e.id_curso_evento)
join projetos.projeto proj on(proj.id_projeto = a.id_atividade  
	and proj.id_tipo_situacao_projeto in (103,105)
	and (extract(year from proj.data_inicio) <= $P{ano} and extract(year from proj.data_fim) >= $P{ano}) )
group by at.descricao
order by at.descricao]]></queryString>

