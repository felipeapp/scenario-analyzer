	<queryString><![CDATA[SELECT DISTINCT A.*,C.NOME AS CURSO_NOME
  FROM (
        SELECT P.ANO,$P{curso} as ID_CURSO,P.TITULO AS PROJETO_TITULO,NULL AS MATRICULA,NULL AS DISCENTE_NOME,PE.NOME AS ORIENTADOR_NOME,
               CASE M.TIPO_PARTICIPACAO
                 WHEN 1 THEN 'SIM'
                 ELSE NULL
                END AS COORDENADOR,
               1 AS TIPO,
               NULL AS VINCULO
          FROM PROJETOS.PROJETO P
         INNER JOIN EXTENSAO.ATIVIDADE A ON A.ID_PROJETO=P.ID_PROJETO
         INNER JOIN PROJETOS.MEMBRO_PROJETO M ON M.ID_PROJETO=A.ID_PROJETO
         INNER JOIN RH.SERVIDOR S ON S.ID_SERVIDOR=M.ID_SERVIDOR
         INNER JOIN COMUM.PESSOA PE ON PE.ID_PESSOA=S.ID_PESSOA 
         LEFT JOIN PROJETOS.FUNCAO_MEMBRO FM ON FM.ID_FUNCAO_MEMBRO=M.ID_FUNCAO_MEMBRO
        WHERE P.ANO = $P{ano}
          AND P.ID_PROJETO IN (SELECT DISTINCT P.ID_PROJETO
                                 FROM PROJETOS.projeto P
                                INNER JOIN EXTENSAO.ATIVIDADE A ON A.ID_PROJETO=P.ID_PROJETO
                                INNER JOIN EXTENSAO.DISCENTE_EXTENSAO D ON D.ID_ATIVIDADE=A.ID_ATIVIDADE
                                INNER JOIN PUBLIC.DISCENTE S ON S.ID_DISCENTE=D.ID_DISCENTE
                                WHERE P.ANO = $P{ano}
                                  AND S.ID_CURSO=$P{curso}
                                  AND D.ATIVO
                              )
        UNION
        SELECT P.ANO,$P{curso} as ID_CURSO,P.TITULO AS PROJETO_TITULO,S.MATRICULA,PE.NOME AS DISCENTE_NOME,NULL AS ORIENTADOR_NOME,
                NULL AS COORDENADOR,
               2 AS TIPO,
               TV.DESCRICAO AS VINCULO
          FROM PROJETOS.PROJETO P
         INNER JOIN EXTENSAO.ATIVIDADE A ON A.ID_PROJETO=P.ID_PROJETO
         INNER JOIN EXTENSAO.DISCENTE_EXTENSAO D ON D.ID_ATIVIDADE=A.ID_ATIVIDADE
         LEFT  JOIN PROJETOS.TIPO_VINCULO_DISCENTE TV ON TV.ID_TIPO_VINCULO_DISCENTE=D.ID_TIPO_VINCULO_DISCENTE
         INNER JOIN PUBLIC.DISCENTE S ON S.ID_DISCENTE=D.ID_DISCENTE
         INNER JOIN COMUM.PESSOA PE ON PE.ID_PESSOA=S.ID_PESSOA 
        WHERE P.ANO = $P{ano}
          AND S.ID_CURSO=$P{curso}
          AND D.ATIVO
     ) A  
   INNER JOIN CURSO C ON C.ID_CURSO=A.ID_CURSO
ORDER BY PROJETO_TITULO,ORIENTADOR_NOME,DISCENTE_NOME]]></queryString>

