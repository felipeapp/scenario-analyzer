	<queryString><![CDATA[select c.classificacao_aprovado, c.argumento_final, p.nome, i.reserva_vagas, o.descricao ,
  ( 
select count(ins.id_processo_seletivo_tecnico) 
from tecnico.resultado_classificacao_candidato_tecnico  
join tecnico.inscricao_processo_seletivo_tecnico ins using (id_inscricao_processo_seletivo_tecnico)  
where ins.id_processo_seletivo_tecnico = i.id_processo_seletivo_tecnico
and ins.id_opcao = i.id_opcao 
  ) as numResultados 
from tecnico.resultado_classificacao_candidato_tecnico c  
join tecnico.inscricao_processo_seletivo_tecnico i using (id_inscricao_processo_seletivo_tecnico)  
join tecnico.pessoa_tecnico p on p.id_pessoa = i.id_pessoa_tecnico 
join tecnico.opcao_polo_grupo o on o.id_opcao_polo_grupo = i.id_opcao 
where i.id_processo_seletivo_tecnico = $P{idProcessoSeletivo}
and (i.id_opcao = $P{opcao} or $P{opcao} = -1)
order by descricao, $P!{ordem}]]></queryString>

