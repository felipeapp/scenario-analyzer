	<queryString><![CDATA[SELECT DISTINCT A.*,C.NOME AS CURSO_NOME
  FROM (
        SELECT PP.ID_PROJETO,PP.COD_PREFIXO||PP.COD_NUMERO AS NUMERO_PROJETO, P.ANO,CAST($P{curso} AS INTEGER) as ID_CURSO,
               P.TITULO AS PROJETO_TITULO,D.MATRICULA,PE2.NOME AS DISCENTE_NOME,
               S.ID_SERVIDOR,
               PE.NOME AS ORIENTADOR_NOME,
               COALESCE(PE.NOME,PE2.NOME,PE3.NOME) AS PESSOA_NOME,
               CASE MP.TIPO_PARTICIPACAO
                 WHEN 1 THEN 'SIM'
                 ELSE NULL
                END AS COORDENADOR,
               CASE COALESCE(D.MATRICULA,-2)
                    WHEN -2 THEN 2
                    ELSE 1
               END AS TIPO,
               CASE MP.TIPO_PARTICIPACAO
                 WHEN 1 THEN 'COORDENADOR'
                 WHEN 2 THEN 'VICE-COORDENADOR'
                 WHEN 3 THEN 'COLABORADOR'
                 ELSE NULL
               END AS VINCULO,
               C.NOME AS DISCENTE_CURSO,
               T.SIGLA AS TURNO
          FROM PROJETOS.projeto P
         INNER JOIN PESQUISA.projeto_pesquisa PP ON PP.ID_PROJETO=P.ID_PROJETO
         INNER JOIN PROJETOS.membro_projeto MP ON MP.ID_PROJETO=P.ID_PROJETO
         LEFT JOIN RH.SERVIDOR S ON S.ID_SERVIDOR=MP.ID_SERVIDOR
         LEFT JOIN COMUM.PESSOA PE ON PE.ID_PESSOA=S.ID_PESSOA 
         
         LEFT JOIN ENSINO.DOCENTE_EXTERNO DE ON DE.ID_DOCENTE_EXTERNO=MP.ID_DOCENTE_EXTERNO
         LEFT JOIN COMUM.PESSOA PE3 ON PE3.ID_PESSOA=DE.ID_PESSOA 
         
         LEFT JOIN PROJETOS.FUNCAO_MEMBRO FM ON FM.ID_FUNCAO_MEMBRO=MP.ID_FUNCAO_MEMBRO
         LEFT JOIN PUBLIC.DISCENTE D ON D.ID_DISCENTE=MP.ID_DISCENTE
         LEFT JOIN COMUM.PESSOA PE2 ON PE2.ID_PESSOA=D.ID_PESSOA 
         LEFT JOIN PUBLIC.CURSO C ON C.ID_CURSO=D.ID_CURSO
         LEFT JOIN GRADUACAO.CURRICULO CU ON CU.ID_CURRICULO=D.ID_CURRICULO
         LEFT JOIN GRADUACAO.MATRIZ_CURRICULAR MC ON MC.ID_MATRIZ_CURRICULAR=CU.ID_MATRIZ
         LEFT JOIN ENSINO.TURNO T ON T.ID_TURNO=MC.ID_TURNO
        WHERE P.ANO = $P{ano}
          AND P.ID_PROJETO IN (SELECT DISTINCT P.ID_PROJETO
                                 FROM PROJETOS.projeto P
                                INNER JOIN PROJETOS.membro_projeto MP ON MP.ID_PROJETO=P.ID_PROJETO
                                INNER JOIN PUBLIC.DISCENTE D ON D.ID_DISCENTE=MP.ID_DISCENTE
                                WHERE P.ANO = $P{ano}
                                  AND D.ID_CURSO=CAST($P{curso} AS INTEGER)
                                  AND MP.ATIVO
                              )
--         AND (NOT ((D.MATRICULA IS NULL) AND (S.ID_SERVIDOR IS NULL)))
     ) A  
   INNER JOIN CURSO C ON C.ID_CURSO=A.ID_CURSO
ORDER BY PROJETO_TITULO,TIPO DESC,PESSOA_NOME]]></queryString>

