			<from-outcome>menuPrincipal</from-outcome>
			<to-view-id>/WEB-INF/jsp/menus/menu_principal.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>*</from-view-id>
