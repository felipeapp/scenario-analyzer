	<queryString><![CDATA[select c.id_curso, c.nome as curso, m1.nome as curso_cidade,
(
select count(*)
from discente d1
where d1.id_curso = c.id_curso
and d1.status in (1, 8)
) as Qtd_Ativos,
(
select count(distinct oa.id_discente)
from graduacao.orientacao_academica oa
inner join discente d2 on d2.id_discente = oa.id_discente
where d2.id_curso = c.id_curso
and d2.status in (1, 8)
and cancelado = false
and tipoorientacao = 'A'
and oa.fim is null
) as qtd_com_orientador_academico,
(
select count(*)
from ensino.matricula_componente mc
inner join discente d3 on d3.id_discente = mc.id_discente
where d3.id_curso = c.id_curso
and mc.ano = $P{ano}
and mc.periodo = $P{periodo}
and mc.id_situacao_matricula in (2,3,4,5,6,7,8)
) as qtd_matriculados_semestre_corrente,
(
select count(*)
from graduacao.solicitacao_matricula sm
inner join discente d4 on d4.id_discente = sm.id_discente
where d4.id_curso = c.id_curso
and sm.ano = $P{ano}
and sm.periodo = $P{periodo}
and sm.status = 8
) as qtd_matriculas_orientadas_semestre_corrente

from curso c
inner join comum.municipio m1 on m1.id_municipio = c.id_municipio
inner join comum.unidade u using (id_unidade)
inner join comum.municipio m2 on m2.id_municipio = u.id_municipio
where c.nivel = 'G'
--group by c.id_curso, c.nome, m1.nome
order by curso_cidade, c.nome, m1.nome]]></queryString>

