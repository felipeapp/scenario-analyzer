	<queryString><![CDATA[SELECT CURSO,
sum(case when id_tipo_curso_stricto = 1 then 1 else 0 end) as MESTRADO_PROFISSIONAL,
sum(case when id_tipo_curso_stricto = 2 then 1 else 0 end) as MESTRADO_ACADEMICO,
sum(case when id_tipo_curso_stricto = 3 then 1 else 0 end) as DOUTORADO
FROM (select u.id_unidade,c.nome as curso, d.nivel, d.tipo, id_tipo_curso_stricto
from public.discente d
inner JOIN public.curso c using (id_curso)
inner join comum.unidade u using (id_unidade)
WHERE (C.ID_UNIDADE=$P{unidade} OR -1=$P{unidade})
AND d.status = 1
AND d.nivel IN ('D','E', 'S')
AND c.nivel IN ('D','E', 'S')) consulta
GROUP BY curso
order by curso]]></queryString>

