	<queryString><![CDATA[SELECT * 
  FROM (
SELECT DISTINCT ME.DESCRICAO AS ME_MODALIDADE, U.ID_UNIDADE,U.SIGLA,C.ID_CURSO,C.NOME||'-'||COALESCE(H.NOME,'')||'-'||COALESCE(T.SIGLA,'') AS CURSO_NOME,
       D.ID_DISCENTE,D.MATRICULA,P.NOME AS PESSOA_NOME,
       (SELECT COUNT(*) 
          FROM ENSINO.MATRICULA_COMPONENTE M2
         WHERE M2.ID_DISCENTE=M.ID_DISCENTE
           AND M2.ANO=$P{ano}
           AND M2.PERIODO=$P{periodo}) AS QTD_MATRICULAS,
       (SELECT COUNT(*) 
          FROM ENSINO.MATRICULA_COMPONENTE M2
         WHERE M2.ID_DISCENTE=M.ID_DISCENTE
           AND M2.ANO=$P{ano}
           AND M2.PERIODO=$P{periodo}
           AND M2.ID_SITUACAO_MATRICULA IN (6,7)) AS QTD_REPROVADO
  FROM ENSINO.MATRICULA_COMPONENTE M
  INNER JOIN PUBLIC.DISCENTE D                     	ON D.ID_DISCENTE=M.ID_DISCENTE
  INNER JOIN GRADUACAO.DISCENTE_GRADUACAO DG        ON DG.ID_DISCENTE_GRADUACAO=D.ID_DISCENTE
  INNER JOIN GRADUACAO.MATRIZ_CURRICULAR MC         ON MC.ID_MATRIZ_CURRICULAR=DG.ID_MATRIZ_CURRICULAR
  LEFT  JOIN GRADUACAO.HABILITACAO H                ON H.ID_HABILITACAO=MC.ID_HABILITACAO
  LEFT  JOIN ENSINO.TURNO T                         ON T.ID_TURNO=MC.ID_TURNO
  INNER JOIN ENSINO.COMPONENTE_CURRICULAR CC 		ON CC.ID_DISCIPLINA=M.ID_COMPONENTE_CURRICULAR
  INNER JOIN ENSINO.TIPO_COMPONENTE_CURRICULAR TCC 	ON TCC.ID_TIPO_DISCIPLINA=CC.ID_TIPO_COMPONENTE
  INNER JOIN PUBLIC.CURSO C  			 			ON C.ID_CURSO=MC.ID_CURSO
  INNER JOIN COMUM.UNIDADE  U 						ON U.ID_UNIDADE=C.ID_UNIDADE
  INNER JOIN COMUM.PESSOA P                		ON P.ID_PESSOA=D.ID_PESSOA
  INNER JOIN COMUM.MODALIDADE_EDUCACAO ME  		ON ME.ID_MODALIDADE_EDUCACAO=C.ID_MODALIDADE_EDUCACAO
  WHERE M.ANO=$P{ano}
    AND M.PERIODO=$P{periodo}
    AND ((U.ID_UNIDADE=$P{unidade}) OR ($P{unidade}=-1))
    AND C.NIVEL='G'
) A
WHERE A.QTD_MATRICULAS=QTD_REPROVADO
ORDER BY ASCII(ME_MODALIDADE),ME_MODALIDADE,SIGLA,CURSO_NOME,PESSOA_NOME]]></queryString>

