	<queryString><![CDATA[select vestibular.processo_seletivo.nome  as concurso,
vestibular.local_aplicacao_prova.nome as local,
pessoa.nome as nome,
pessoa.cpf_cnpj as cpf,
discente.matricula as matricula,
case when reserva then 'Reserva' else 'Titular' end as titular
from vestibular.processo_seletivo inner join vestibular.fiscal using (id_processo_seletivo)
inner join vestibular.local_aplicacao_prova using (id_local_aplicacao_prova)
inner join comum.pessoa using (id_pessoa)
inner join discente using (id_discente)
where fiscal.id_servidor is null
and  fiscal.id_processo_seletivo = $P{concurso}
and presente_reuniao = true
union
select vestibular.processo_seletivo.nome as concurso,
vestibular.local_aplicacao_prova.nome as local,
pessoa.nome as nome,
pessoa.cpf_cnpj as cpf,
rh.servidor.siape as matricula,
case when reserva then 'Reserva' else 'Titular' end as titular
from vestibular.processo_seletivo inner join vestibular.fiscal using (id_processo_seletivo)
inner join vestibular.local_aplicacao_prova using (id_local_aplicacao_prova)
inner join comum.pessoa using (id_pessoa)
inner join rh.servidor using (id_servidor)
where fiscal.id_discente is null
and  fiscal.id_processo_seletivo = $P{concurso}
and presente_reuniao = true
order by local, nome;]]></queryString>

