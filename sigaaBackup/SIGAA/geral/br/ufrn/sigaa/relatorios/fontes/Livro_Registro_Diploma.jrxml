	<parameter name="ifes" isForPrompting="false" class="java.lang.String"/>
	<queryString><![CDATA[select registro_diploma.ativo as ativo,
pessoa.nome_oficial as nome,
pessoa.data_nascimento,
pessoa.sexo,
pessoa.nome_pai,
pessoa.nome_mae,
coalesce(pessoa.numero_identidade, '0')||'-'||coalesce(pessoa.orgao_expedicao_identidade,'NI')||'/'||coalesce(unidade_federativa.sigla,'NI') as identidade,
curso.nome as curso,
municipio.nome as municipio,
(select portaria_decreto from graduacao.reconhecimento where id_matriz = matriz_curricular.id_matriz_curricular order by data_publicacao desc limit 1) as portaria_decreto,
(select data_publicacao from graduacao.reconhecimento where id_matriz = matriz_curricular.id_matriz_curricular order by data_publicacao desc limit 1) as data_publicao_decreto,
(select data_decreto from graduacao.reconhecimento where id_matriz = matriz_curricular.id_matriz_curricular order by data_publicacao desc limit 1) as data_decreto,
registro_diploma.data_colacao,
registro_diploma.data_expedicao,
registro_diploma.data_registro,
(select array_to_string(
 array(select observacao
   from diploma.observacao_registro_diploma ord
   where ord.id_registro_diploma = registro_diploma.id_registro_diploma
   and ativo = true
   order by data_observacao
 ))) as observacoes,
coalesce(grau_academico.titulo_masculino, curso.titulacao_masculino) as grau,
habilitacao.nome as habilitacao,
registro_diploma.processo,
livro_registro_diploma.titulo as titulo_livro,
folha_registro_diploma.numero_folha,
registro_diploma.numero_registro,
case when municipio_nascimento.nome is null then pessoa.municipio_naturalidade_outro else municipio_nascimento.nome end as municipio_nascimento, 
uf_nascimento.descricao as uf_nascimento,
uf_nascimento.sigla as sigla_uf_nascimento,
case when pessoa.id_uf_naturalidade in (25, 29) then 'a'
     when pessoa.id_uf_naturalidade in (26,11,35,31,27,28,14,52,42) then 'e'
     else 'o'
     end as artigo_uf_nascimento,
livro_registro_diploma.instituicao,
livro_registro_diploma.registro_externo as externo,
responsavel_assinatura.nome_reitor as reitor,
responsavel_assinatura.nome_diretor_unidade_diploma as diretor,
responsavel.nome as responsavel
from diploma.registro_diploma
inner join discente using (id_discente)
left join graduacao.discente_graduacao on (id_discente = discente_graduacao.id_discente_graduacao)
left join stricto_sensu.discente_stricto using (id_discente)
inner join comum.registro_entrada on (registro_diploma.id_registro_entrada = registro_entrada.id_entrada)
inner join comum.usuario using (id_usuario)
inner join comum.pessoa responsavel on (usuario.id_pessoa = responsavel.id_pessoa)
inner join diploma.folha_registro_diploma using (id_folha_registro_diploma)
inner join diploma.livro_registro_diploma using (id_livro_registro_diploma)
left join graduacao.matriz_curricular using (id_matriz_curricular)
inner join curso on (discente.id_curso = curso.id_curso)
inner join comum.municipio using (id_municipio)
left join diploma.responsavel_assinatura using (id_responsavel_assinatura)
left join ensino.grau_academico on (matriz_curricular.id_grau_academico = grau_academico.id_grau_academico)
left join graduacao.habilitacao using (id_habilitacao)
inner join comum.pessoa on (discente.id_pessoa = pessoa.id_pessoa)
left join comum.identidade on (pessoa.id_identidade = identidade.id_identidade)
left join comum.unidade_federativa on (unidade_federativa.id_unidade_federativa = pessoa.id_uf_identidade)
left join comum.municipio municipio_nascimento on (municipio_nascimento.id_municipio = pessoa.id_municipio_naturalidade)
left join comum.unidade_federativa uf_nascimento on (pessoa.id_uf_naturalidade = uf_nascimento.id_unidade_federativa)
where livro_registro_diploma.id_livro_registro_diploma = $P{idLivro}
order by numero_folha, indice, numero_registro;]]></queryString>

