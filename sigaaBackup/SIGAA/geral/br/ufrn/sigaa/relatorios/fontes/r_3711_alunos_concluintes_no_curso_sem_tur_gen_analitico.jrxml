	<parameter name="unidade" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(440)]]></defaultValueExpression>
	</parameter>
	<parameter name="ano" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(2006)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT ME_MODALIDADE,ID_UNIDADE,SIGLA,ID_CURSO,CURSO_NOME,ID_DISCENTE,MATRICULA,PESSOA_NOME,SEXO,
       DATA_OCORRENCIA_CONCLUSAO,
       DATA_OCORRENCIA_GRADUANDO,
       CASE
           WHEN DATA_OCORRENCIA_CONCLUSAO IS NULL THEN 'GRADUANDO'
           ELSE 'CONCLUIDO'
       END AS STATUS
 FROM (
SELECT DISTINCT ME.DESCRICAO AS ME_MODALIDADE,U.ID_UNIDADE,U.SIGLA,C.ID_CURSO,C.NOME AS CURSO_NOME,
       D.ID_DISCENTE,D.MATRICULA,P.NOME AS PESSOA_NOME,P.SEXO,
                          (SELECT DATA_OCORRENCIA
                             FROM ENSINO.MOVIMENTACAO_ALUNO MA 
                            WHERE MA.ID_DISCENTE=D.ID_DISCENTE
                              AND MA.ano_referencia=$P{ano}
                              AND MA.ID_TIPO_MOVIMENTACAO_ALUNO=1 
                              AND ((MA.ATIVO) OR (MA.ATIVO IS NULL)) LIMIT 1) AS DATA_OCORRENCIA_CONCLUSAO,
                          (SELECT DATA
                             FROM COMUM.ALTERACAO_STATUS_ALUNO ASA
                            WHERE ASA.ID_DISCENTE=D.ID_DISCENTE
                              AND ASA.STATUS=9 LIMIT 1) AS DATA_OCORRENCIA_GRADUANDO
   FROM PUBLIC.DISCENTE D
  INNER JOIN PUBLIC.CURSO C  				ON C.ID_CURSO=D.ID_CURSO
  INNER JOIN COMUM.UNIDADE  U 				ON U.ID_UNIDADE=C.ID_UNIDADE
  INNER JOIN COMUM.PESSOA P                ON P.ID_PESSOA=D.ID_PESSOA
  INNER JOIN COMUM.MODALIDADE_EDUCACAO ME  ON ME.ID_MODALIDADE_EDUCACAO=C.ID_MODALIDADE_EDUCACAO
 WHERE ((U.ID_UNIDADE=$P{unidade}) OR ($P{unidade}=-1))
    AND C.NIVEL='G'
    and ((d.status = 3 and d.id_discente in ( 
                                select id_discente 
                                from ensino.movimentacao_aluno 
                                where  ANO_referencia = $P{ano}  
                                and PERIODO_referencia in (1,3) 
                                and id_tipo_movimentacao_aluno = 1 
                                and ativo = trueValue()) 
          or d.status = 9  and f_discente_graduando(id_discente, $P{ano} , 1, null) = trueValue()) 
     or (d.status = 3 and d.id_discente in (
                                select id_discente 
                                from ensino.movimentacao_aluno 
                                where  ANO_referencia = $P{ano}  
                                  and PERIODO_referencia in (2,4) 
                                  and id_tipo_movimentacao_aluno = 1 
                                  and ativo = trueValue()) 
         or d.status = 9 and f_discente_graduando(id_discente, $P{ano}, 2, null) = trueValue()))  
) A        
ORDER BY ASCII(ME_MODALIDADE),ME_MODALIDADE,SIGLA,CURSO_NOME,PESSOA_NOME]]></queryString>

