	<queryString><![CDATA[SELECT DESCRICAO,MASCULINO,FEMININO,(MASCULINO+FEMININO) AS TOTAL
  FROM (
SELECT 1 AS ORDEM,'PROFESSOR 3º GRAU' AS DESCRICAO,(SELECT COUNT(*)
                                      FROM FUNCIONAL.HISTORICO_SERVIDOR H
                                     INNER JOIN RH.SERVIDOR S ON S.ID_SERVIDOR=H.ID_SERVIDOR
                                     INNER JOIN COMUM.PESSOA  P ON P.ID_PESSOA=S.ID_PESSOA
                                     LEFT  JOIN COMUM.UNIDADE U ON U.ID_UNIDADE=S.ID_UNIDADE
                                     WHERE H.ANO=$P{ano} AND H.MES=$P{mes}
                                       AND S.ID_CATEGORIA=1 /*DOCENTE*/
                                       AND P.SEXO='M'
                                       AND S.ID_SITUACAO IN (1,20) /*ATIVO/CELETISTA*/
                                       AND S.ID_CARGO NOT in (60011, 60012, 60013)
                                       AND S.ID_SERVIDOR NOT IN (SELECT ID_SERVIDOR 
                                                                   FROM FUNCIONAL.OCORRENCIA_FUNCIONAL OC
                                                                   LEFT JOIN FUNCIONAL.OCORRENCIA O ON O.ID_OCORRENCIA=OC.ID_OCORRENCIA
									LEFT JOIN FUNCIONAL.ASSOCIACAO_GRUPO_OCORRENCIA AGO ON O.ID_OCORRENCIA=AGO.ID_OCORRENCIA
                                                                  WHERE OC.ID_SERVIDOR=S.ID_SERVIDOR 
                                                                    AND AGO.ID_GRUPO_OCORRENCIA=2
                                                                    AND EXTRACT(YEAR FROM OC.DATA)<=$P{ano} 
                                                                    AND EXTRACT(MONTH FROM OC.DATA)<=$P{mes})
                           ) AS MASCULINO,
                           (SELECT COUNT(*)
                                      FROM FUNCIONAL.HISTORICO_SERVIDOR H
                                     INNER JOIN RH.SERVIDOR S ON S.ID_SERVIDOR=H.ID_SERVIDOR
                                     INNER JOIN COMUM.PESSOA  P ON P.ID_PESSOA=S.ID_PESSOA
                                     LEFT  JOIN COMUM.UNIDADE U ON U.ID_UNIDADE=S.ID_UNIDADE
                                     WHERE H.ANO=$P{ano} AND H.MES=$P{mes}
                                       AND S.ID_CATEGORIA=1 /*DOCENTE*/
                                       AND P.SEXO='F'
                                       AND S.ID_SITUACAO IN (1,20) /*ATIVO/CELETISTA*/
                                       AND S.ID_CARGO NOT in (60011, 60012, 60013)
                                       AND S.ID_SERVIDOR NOT IN (SELECT ID_SERVIDOR 
                                                                   FROM FUNCIONAL.OCORRENCIA_FUNCIONAL OC
                                                                   LEFT JOIN FUNCIONAL.OCORRENCIA O ON O.ID_OCORRENCIA=OC.ID_OCORRENCIA
								LEFT JOIN FUNCIONAL.ASSOCIACAO_GRUPO_OCORRENCIA AGO ON O.ID_OCORRENCIA=AGO.ID_OCORRENCIA
                                                                  WHERE OC.ID_SERVIDOR=S.ID_SERVIDOR 
                                                                    AND AGO.ID_GRUPO_OCORRENCIA=2
                                                                    AND EXTRACT(YEAR FROM OC.DATA)<=$P{ano} 
                                                                    AND EXTRACT(MONTH FROM OC.DATA)<=$P{mes})
                           ) AS FEMININO
UNION
SELECT 2 AS ORDEM,'PROFESSOR 1º E 2º GRAUS' AS DESCRICAO,(SELECT COUNT(*)
                                      FROM FUNCIONAL.HISTORICO_SERVIDOR H
                                     INNER JOIN RH.SERVIDOR S ON S.ID_SERVIDOR=H.ID_SERVIDOR
                                     INNER JOIN COMUM.PESSOA  P ON P.ID_PESSOA=S.ID_PESSOA
                                     LEFT  JOIN COMUM.UNIDADE U ON U.ID_UNIDADE=S.ID_UNIDADE
                                     WHERE H.ANO=$P{ano} AND H.MES=$P{mes}
                                       AND S.ID_CATEGORIA=1 /*DOCENTE*/
                                       AND P.SEXO='M'
                                       AND S.ID_SITUACAO IN (1,20) /*ATIVO/CELETISTA*/
                                       AND S.ID_CARGO in (60011, 60012, 60013)
                                       AND S.ID_SERVIDOR NOT IN (SELECT ID_SERVIDOR 
                                                                   FROM FUNCIONAL.OCORRENCIA_FUNCIONAL OC
                                                                   LEFT JOIN FUNCIONAL.OCORRENCIA O ON O.ID_OCORRENCIA=OC.ID_OCORRENCIA
									LEFT JOIN FUNCIONAL.ASSOCIACAO_GRUPO_OCORRENCIA AGO ON O.ID_OCORRENCIA=AGO.ID_OCORRENCIA
                                                                  WHERE OC.ID_SERVIDOR=S.ID_SERVIDOR 
                                                                    AND AGO.ID_GRUPO_OCORRENCIA=2
                                                                    AND EXTRACT(YEAR FROM OC.DATA)<=$P{ano} 
                                                                    AND EXTRACT(MONTH FROM OC.DATA)<=$P{mes})
                           ) AS MASCULINO,
                           (SELECT COUNT(*)
                                      FROM FUNCIONAL.HISTORICO_SERVIDOR H
                                     INNER JOIN RH.SERVIDOR S ON S.ID_SERVIDOR=H.ID_SERVIDOR
                                     INNER JOIN COMUM.PESSOA  P ON P.ID_PESSOA=S.ID_PESSOA
                                     LEFT  JOIN COMUM.UNIDADE U ON U.ID_UNIDADE=S.ID_UNIDADE
                                     WHERE H.ANO=$P{ano} AND H.MES=$P{mes}
                                       AND S.ID_CATEGORIA=1 /*DOCENTE*/
                                       AND P.SEXO='F'
                                       AND S.ID_SITUACAO IN (1,20) /*ATIVO/CELETISTA*/
                                       AND S.ID_CARGO in (60011, 60012, 60013)
                                       AND S.ID_SERVIDOR NOT IN (SELECT ID_SERVIDOR 
                                                                   FROM FUNCIONAL.OCORRENCIA_FUNCIONAL OC
                                                                   LEFT JOIN FUNCIONAL.OCORRENCIA O ON O.ID_OCORRENCIA=OC.ID_OCORRENCIA
									LEFT JOIN FUNCIONAL.ASSOCIACAO_GRUPO_OCORRENCIA AGO ON O.ID_OCORRENCIA=AGO.ID_OCORRENCIA
                                                                  WHERE OC.ID_SERVIDOR=S.ID_SERVIDOR 
                                                                    AND AGO.ID_GRUPO_OCORRENCIA=2
                                                                    AND EXTRACT(YEAR FROM OC.DATA)<=$P{ano} 
                                                                    AND EXTRACT(MONTH FROM OC.DATA)<=$P{mes})
                           ) AS FEMININO
) A
ORDER BY ORDEM]]></queryString>

