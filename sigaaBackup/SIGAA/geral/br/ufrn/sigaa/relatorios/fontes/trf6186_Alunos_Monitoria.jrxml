	<queryString><![CDATA[SELECT DISTINCT A.*,C.NOME AS CURSO_NOME
  FROM (
SELECT D.ID_CURSO,PRJ.ANO,PRJ.ID_PROJETO,PRJ.TITULO AS PROJETO_TITULO,
       NULL AS DISCENTE_NOME,
       P2.NOME AS ORIENTADOR_NOME,
       CASE ED.COORDENADOR
         WHEN TRUE THEN 'SIM'
         ELSE NULL
       END AS COORDENADOR,
       NULL AS VINCULO,1 AS TIPO,
       NULL AS MATRICULA
FROM MONITORIA.discente_monitoria DM
INNER JOIN PUBLIC.discente D ON D.ID_DISCENTE=DM.ID_DISCENTE
INNER JOIN COMUM.pessoa P ON P.ID_PESSOA=D.ID_PESSOA
INNER JOIN MONITORIA.projeto_monitoria PM ON PM.ID_PROJETO_MONITORIA=DM.ID_PROJETO_MONITORIA
INNER JOIN PROJETOS.projeto PRJ ON PRJ.ID_PROJETO=PM.ID_PROJETO
INNER JOIN MONITORIA.EQUIPE_DOCENTE ED ON ED.ID_PROJETO_MONITORIA=PM.ID_PROJETO_MONITORIA
INNER JOIN RH.SERVIDOR S ON S.ID_SERVIDOR=ED.ID_SERVIDOR
INNER JOIN COMUM.PESSOA P2 ON P2.ID_PESSOA=S.ID_PESSOA
WHERE D.ID_CURSO=$P{curso}
  AND PRJ.ANO=$P{ano}
  AND DM.ID_SITUACAO_DISCENTE_MONITORIA IN (5,6)
  AND DM.ATIVO
  AND ED.ATIVO
UNION
SELECT D.ID_CURSO,PRJ.ANO,PRJ.ID_PROJETO,PRJ.TITULO AS PROJETO_TITULO,
       P.NOME AS DISCENTE_NOME,
       NULL AS ORIENTADOR_NOME,
       NULL AS COORDENADOR,
       CASE DM.ID_TIPO_VINCULO_DISCENTE
           WHEN 1 THEN 'VOLUNTARIO'
           WHEN 2 THEN 'BOLSISTA'
           ELSE 'NAO CLASSIFICADO'
       END AS VINCULO,2 AS TIPO,
       D.MATRICULA
FROM MONITORIA.discente_monitoria DM
INNER JOIN PUBLIC.discente D ON D.ID_DISCENTE=DM.ID_DISCENTE
INNER JOIN COMUM.pessoa P ON P.ID_PESSOA=D.ID_PESSOA
INNER JOIN MONITORIA.projeto_monitoria PM ON PM.ID_PROJETO_monitoria=DM.ID_PROJETO_MONITORIA
INNER JOIN PROJETOS.projeto PRJ ON PRJ.ID_PROJETO=PM.ID_PROJETO
INNER JOIN MONITORIA.EQUIPE_DOCENTE ED ON ED.ID_PROJETO_MONITORIA=PM.ID_PROJETO_MONITORIA
INNER JOIN RH.SERVIDOR S ON S.ID_SERVIDOR=ED.ID_SERVIDOR
INNER JOIN COMUM.PESSOA P2 ON P2.ID_PESSOA=S.ID_PESSOA
WHERE D.ID_CURSO=$P{curso}
  AND PRJ.ANO=$P{ano}
  AND DM.ID_SITUACAO_DISCENTE_MONITORIA IN (5,6)
  AND DM.ATIVO
) A
   INNER JOIN CURSO C ON C.ID_CURSO=A.ID_CURSO
ORDER BY PROJETO_TITULO,ID_PROJETO,ORIENTADOR_NOME,DISCENTE_NOME]]></queryString>

