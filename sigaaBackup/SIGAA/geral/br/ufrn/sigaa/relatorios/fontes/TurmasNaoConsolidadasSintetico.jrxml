	<queryString><![CDATA[select centro.nome as centro, centro.sigla, departamento.id_unidade, departamento.nome as departamento, 
sum(case when id_situacao_turma = 3 then 1 else 0 end) as consolidadas,
count(*) as total
from ensino.turma
inner join ensino.componente_curricular using (id_disciplina)
inner join comum.unidade departamento using (id_unidade)
inner join comum.unidade centro on (departamento.id_gestora = centro.id_unidade)
where turma.ano = $P{ano} and turma.periodo = $P{periodo}
and turma.agrupadora = falseValue()
and nivel = 'G'
and (centro.id_unidade = $P{unidade} or $P{unidade}=-1)
and id_situacao_turma not in (4,5,6)
and exists (
		select id_turma from ensino.matricula_componente 
				where id_turma = turma.id_turma
				and id_situacao_matricula in (4,3,2,6,7,9,5,15)
 	   )
group by 1,2, 3, 4
having (count(*) - sum(case when id_situacao_turma = 3 then 1 else 0 end) != 0)
ORDER BY CENTRO, DEPARTAMENTO]]></queryString>

