	<queryString><![CDATA[SELECT U.ID_UNIDADE,U.NOME AS NOME_UNIDADE,
       S.SIAPE,P.NOME,TEO.TITULO,
       CASE
          WHEN TEO.ID_DISCENTE IS NULL THEN TEO.ORIENTANDO
          ELSE (SELECT NOME 
                  FROM COMUM.PESSOA P2 
                 INNER JOIN PUBLIC.DISCENTE D ON D.ID_PESSOA=P2.ID_PESSOA
                 WHERE D.ID_DISCENTE=TEO.ID_DISCENTE)
       END AS ORIENTANDO,
       TEO.DATA_PUBLICACAO
  FROM PRODOCENTE.TESE_ORIENTADA TEO
  INNER JOIN RH.SERVIDOR S ON S.ID_SERVIDOR=TEO.ID_SERVIDOR
  INNER JOIN COMUM.PESSOA P ON P.ID_PESSOA=S.ID_PESSOA
  INNER JOIN COMUM.UNIDADE U ON U.ID_UNIDADE=S.ID_UNIDADE
 WHERE (($P{unidade}=-1) or (S.ID_UNIDADE=$P{unidade}))
   AND EXTRACT(YEAR FROM TEO.DATA_PUBLICACAO)=$P{ano}
   AND TEO.ORIENTACAO='O'
 ORDER BY U.NOME,P.NOME]]></queryString>

