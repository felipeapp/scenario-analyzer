	<queryString><![CDATA[SELECT E.DESCRICAO AS DESCRICAO_EDITAL,T.DESCRICAO,SUM(A.BOLSAS_SOLICITADAS) AS BOLSAS_SOLICITADAS,SUM(A.BOLSAS_CONCEDIDAS) AS BOLSAS_CONCEDIDAS
  FROM EXTENSAO.ATIVIDADE A
 INNER JOIN PROJETOS.PROJETO P ON P.ID_PROJETO=A.ID_ATIVIDADE
 INNER JOIN PROJETOS.EDITAL E ON E.ID_EDITAL=A.ID_EDITAL
 INNER JOIN EXTENSAO.TIPO_ATIVIDADE_EXTENSAO T ON T.ID_TIPO_ATIVIDADE_EXTENSAO=A.ID_TIPO_ATIVIDADE_EXTENSAO
 WHERE (A.ID_EDITAL=$P{edital} OR $P{edital} IS NULL)
   AND (CAST($P{inicio} AS DATE) IS NULL OR CAST($P{fim} AS DATE) IS NULL OR 
          (CAST($P{inicio} AS DATE)<=P.DATA_INICIO AND (CAST($P{fim} AS DATE)>=P.DATA_FIM OR P.DATA_FIM IS NULL))
       )
GROUP BY E.DESCRICAO,T.DESCRICAO
ORDER BY T.DESCRICAO]]></queryString>

