	<queryString><![CDATA[SELECT DISTINCT U.ID_UNIDADE,U.SIGLA,C.NOME,H.NOME AS HABILITACAO,
       UPPER(R.PORTARIA_DECRETO) AS PORTARIA_DECRETO,R.DATA_PUBLICACAO AS DOU, R.DATA_DECRETO
  FROM GRADUACAO.MATRIZ_CURRICULAR MC
  INNER JOIN GRADUACAO.RECONHECIMENTO R     ON R.ID_MATRIZ=MC.ID_MATRIZ_CURRICULAR 
  LEFT  JOIN ENSINO.GRAU_ACADEMICO G		ON G.ID_GRAU_ACADEMICO=MC.ID_GRAU_ACADEMICO
  LEFT  JOIN GRADUACAO.HABILITACAO H		ON H.ID_HABILITACAO=MC.ID_HABILITACAO
  INNER JOIN PUBLIC.CURSO C 				ON C.ID_CURSO=MC.ID_CURSO
  INNER JOIN COMUM.UNIDADE  U 				ON U.ID_UNIDADE=C.ID_UNIDADE
 WHERE ((U.ID_UNIDADE=$P{unidade}) OR ($P{unidade}=-1))
    AND C.NIVEL='G'
    AND MC.ATIVO
 ORDER BY U.SIGLA,C.NOME,H.NOME]]></queryString>

