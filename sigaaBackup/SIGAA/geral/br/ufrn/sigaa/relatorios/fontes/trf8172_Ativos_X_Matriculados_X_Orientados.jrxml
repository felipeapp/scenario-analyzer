	<queryString><![CDATA[SELECT (SELECT NOME 
          FROM PUBLIC.CURSO C
         WHERE C.ID_CURSO=$P{curso}
       ) AS CURSO,    
       ATIVOS,MATRICULADOS,
       ATIVOS-MATRICULADOS AS DIFERENCA_M,
	CASE
		WHEN ATIVOS>0 THEN CAST(MATRICULADOS*100.0/ATIVOS AS NUMERIC(5,1)) 
		ELSE 0
	END AS PERC_MATRICULADOS,
	ORIENTADOS,
	ATIVOS-ORIENTADOS AS DIFERENCA_O,
	CASE
		WHEN ATIVOS>0 THEN CAST(ORIENTADOS*100.0/ATIVOS AS NUMERIC(5,1)) 
		ELSE 0
	END AS PERC_ORIENTADOS
FROM (
SELECT (SELECT COUNT(*) 
          FROM PUBLIC.DISCENTE D  
         WHERE D.ID_CURSO=$P{curso}
           AND D.STATUS IN (1,2,8)) AS ATIVOS,
       (SELECT COUNT(DISTINCT M.ID_DISCENTE)
          FROM GRADUACAO.SOLICITACAO_MATRICULA M
         INNER JOIN PUBLIC.DISCENTE D ON D.ID_DISCENTE=M.ID_DISCENTE
         WHERE D.ID_CURSO=$P{curso}
           AND D.STATUS IN (1,2,8)
           AND M.ID_DISCENTE=D.ID_DISCENTE
           AND M.ANO=$P{ano}
           AND M.PERIODO=$P{periodo}
       ) AS MATRICULADOS,
       (SELECT COUNT(*) 
          FROM PUBLIC.DISCENTE D  
         INNER JOIN GRADUACAO.ORIENTACAO_ACADEMICA O ON O.ID_DISCENTE=D.ID_DISCENTE
         WHERE D.ID_CURSO=$P{curso}
           AND D.STATUS IN (1,2,8)
           AND O.DATA_FINALIZACAO IS NULL) AS ORIENTADOS
) A]]></queryString>

