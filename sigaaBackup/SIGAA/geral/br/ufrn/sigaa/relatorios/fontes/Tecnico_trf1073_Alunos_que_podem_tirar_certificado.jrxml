	<queryString><![CDATA[SELECT DISTINCT DESCRICAO,MATRICULA,ID_DISCENTE,NOME,ID_CURSO_TECNICO,HABILITACAO,TEXTO_CERTIFICADO
  FROM (
        SELECT DISTINCT D2.MATRICULA,D2.ID_DISCENTE,P.NOME,Q.ID_QUALIFICACAO,Q.DESCRICAO,Q.ID_CURSO_TECNICO,Q.HABILITACAO,Q.TEXTO_CERTIFICADO
          FROM ENSINO.matricula_componente MC
         INNER JOIN PUBLIC.DISCENTE D2 ON D2.ID_DISCENTE=MC.ID_DISCENTE
         INNER JOIN COMUM.PESSOA P ON P.ID_PESSOA=D2.ID_PESSOA
         INNER JOIN tecnico.MODULO_DISCIPLINA MD ON MD.ID_DISCIPLINA=MC.ID_COMPONENTE_CURRICULAR
         INNER JOIN tecnico.MODULO_QUALIFICACAO MQ ON MQ.ID_MODULO=MD.ID_MODULO
         INNER JOIN TECNICO.QUALIFICACAO Q ON Q.ID_QUALIFICACAO=MQ.ID_QUALIFICACAO
         WHERE /*D2.ID_DISCENTE=500351
           AND*/  ANO=$P{ano}
           AND D2.NIVEL='T'
           AND MC.ID_SITUACAO_MATRICULA in (4,21,22,23)
        ) A
ORDER BY DESCRICAO,NOME]]></queryString>

