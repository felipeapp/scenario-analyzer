	<queryString><![CDATA[SELECT ID_ESCOLARIDADE_PAI,ESCOLARIDADE_DESCRICAO,
      ( SELECT COUNT(*)
         FROM SAE.BOLSA_AUXILIO_PERIODO P2
        INNER JOIN SAE.BOLSA_AUXILIO            B2 ON B2.ID_BOLSA_AUXILIO=P2.ID_BOLSA_AUXILIO
        INNER JOIN SAE.SITUACAO_SOCIO_ECONOMICA S2 ON S2.ID_SITUACAO_SOCIO_ECONOMICA=B2.ID_SITUACAO_SOCIO_ECONOMICA
        WHERE P2.ANO=$P{ano}
          AND P2.PERIODO=$P{periodo}
          AND S2.ID_ESCOLARIDADE_PAI=A.ID_ESCOLARIDADE_PAI
          AND B2.SITUACAO=1 -- 1-EM ANALISE, 2-DEFERIDA/CONTEMPLADA, 3-DEFERIDA(FILA DE ESPERA), 4-INDEFERIDA 
      ) AS SOLICITACAO,
      ( SELECT COUNT(*)
         FROM SAE.BOLSA_AUXILIO_PERIODO P2
        INNER JOIN SAE.BOLSA_AUXILIO            B2 ON B2.ID_BOLSA_AUXILIO=P2.ID_BOLSA_AUXILIO
        INNER JOIN SAE.SITUACAO_SOCIO_ECONOMICA S2 ON S2.ID_SITUACAO_SOCIO_ECONOMICA=B2.ID_SITUACAO_SOCIO_ECONOMICA
        WHERE P2.ANO=$P{ano}
          AND P2.PERIODO=$P{periodo}
          AND S2.ID_ESCOLARIDADE_PAI=A.ID_ESCOLARIDADE_PAI
          AND B2.SITUACAO=2 -- 1-EM ANALISE, 2-DEFERIDA/CONTEMPLADA, 3-DEFERIDA(FILA DE ESPERA), 4-INDEFERIDA 
      ) AS DEFERIDA
 FROM (
        SELECT DISTINCT ID_ESCOLARIDADE_PAI,
                        CASE
                           WHEN ID_ESCOLARIDADE_PAI= 1 THEN 'ANALFABETO'
                           WHEN ID_ESCOLARIDADE_PAI= 6 THEN 'ENSINO FUNDAMENTAL'
                           WHEN ID_ESCOLARIDADE_PAI= 8 THEN 'ENSINO MÉDIO'
                           WHEN ID_ESCOLARIDADE_PAI=10 THEN 'ENSINO SUPERIOR'
                           WHEN ID_ESCOLARIDADE_PAI=12 THEN 'ESPECIALIZAÇÃO'
                           WHEN ID_ESCOLARIDADE_PAI=13 THEN 'MESTRADO'
                           WHEN ID_ESCOLARIDADE_PAI=14 THEN 'DOUTORADO'
                           ELSE 'NÃO INFORMADO'
                        END AS ESCOLARIDADE_DESCRICAO 
          FROM SAE.BOLSA_AUXILIO_PERIODO P
         INNER JOIN SAE.BOLSA_AUXILIO            B ON B.ID_BOLSA_AUXILIO=P.ID_BOLSA_AUXILIO
         INNER JOIN SAE.SITUACAO_SOCIO_ECONOMICA S ON S.ID_SITUACAO_SOCIO_ECONOMICA=B.ID_SITUACAO_SOCIO_ECONOMICA
         WHERE P.ANO=$P{ano}
           AND P.PERIODO=$P{periodo}
         ORDER BY ID_ESCOLARIDADE_PAI
      ) A]]></queryString>

