	<queryString><![CDATA[--8433
SELECT * FROM (
SELECT C.NOME AS CURSO,COALESCE(ATE.DESCRICAO,'') AS ESPECIALIZACAO, ECT.PRAZO_MAX_CONCLUSAO,D.MATRICULA,P.NOME,
       CASE
          WHEN ((D.ANO_INGRESSO||''||D.PERIODO_INGRESSO)>($P{ano}||''||$P{periodo})) THEN ' '
          WHEN (SELECT COUNT(*) 
                  FROM ENSINO.MATRICULA_COMPONENTE M
                  WHERE M.ID_DISCENTE=D.ID_DISCENTE
                    AND (D.ANO_INGRESSO||''||D.PERIODO_INGRESSO)<=($P{ano}||''||$P{periodo})
                    AND M.ANO=$P{ano}
                    AND M.PERIODO=$P{periodo}
               )>0 THEN ' '
          ELSE 'X'
       END AS SEM_MATRICULA,
       CASE 
          WHEN (SELECT COUNT(*)
                  FROM (SELECT DISTINCT M.ID_COMPONENTE_DETALHES,COUNT(*) AS QTD
                         FROM ENSINO.MATRICULA_COMPONENTE M
                         WHERE M.ID_DISCENTE=D.ID_DISCENTE
                           AND M.ID_SITUACAO_MATRICULA IN (6,7)
                         GROUP BY 1  
                       ) A
                 WHERE A.QTD>=3
               )>0 THEN 'X'
          ELSE ' '
       END AS REPROVADO_3_VEZES,
       CASE 
          WHEN (SELECT COUNT(DISTINCT M.ID_COMPONENTE_DETALHES)      
                  FROM ENSINO.MATRICULA_COMPONENTE M
                  WHERE M.ID_DISCENTE=D.ID_DISCENTE
                    AND M.ID_SITUACAO_MATRICULA IN (6,7)
               )>=4 THEN 'X'
          ELSE ' '
       END AS REPROVACOES_4_VEZES,
       CASE
          WHEN (SELECT COUNT(*) 
                  FROM (SELECT SUM(CH_TOTAL/2) AS FALTAS_LIMITE, SUM(NUMERO_FALTAS) AS FALTAS
                          FROM PUBLIC.DISCENTE D2
                          INNER JOIN ENSINO.MATRICULA_COMPONENTE M ON M.ID_DISCENTE=D2.ID_DISCENTE
                          INNER JOIN ENSINO.COMPONENTE_CURRICULAR_DETALHES CCD ON CCD.ID_COMPONENTE=M.ID_COMPONENTE_CURRICULAR
                        WHERE D2.ID_DISCENTE=D.ID_DISCENTE
                          AND M.ANO=$P{ano}
                          AND M.PERIODO=$P{periodo}
                       ) A
                WHERE A.FALTAS>=A.FALTAS_LIMITE
               )>0 THEN 'X'
          ELSE ' '
       END AS FALTOU_DMAIS,
       CASE
          WHEN D.PRAZO_CONCLUSAO<(SELECT  CAST(ANO||''||PERIODO AS INTEGER) AS PERIODO
                                    FROM COMUM.CALENDARIO_ACADEMICO
                                   WHERE ID_UNIDADE=284
                                   ORDER BY INICIOPERIODOLETIVO DESC
                                   LIMIT 1
                                  ) THEN 'X'
          ELSE ' '
       END AS EXCEDEU_PRAZO   
  FROM PUBLIC.DISCENTE D
  INNER JOIN TECNICO.DISCENTE_TECNICO DT ON DT.ID_DISCENTE=D.ID_DISCENTE
  LEFT  JOIN TECNICO.TURMA_ENTRADA_TECNICO TE ON TE.ID_TURMA_ENTRADA=DT.ID_TURMA_ENTRADA
  LEFT  JOIN TECNICO.ESPECIALIZACAO_TURMA_ENTRADA ATE ON ATE.ID_ESPECIALIZACAO_TURMA_ENTRADA=TE.ID_ESPECIALIZACAO_TURMA_ENTRADA
  INNER JOIN PUBLIC.CURSO C ON C.ID_CURSO=D.ID_CURSO
  INNER JOIN COMUM.PESSOA P ON P.ID_PESSOA=D.ID_PESSOA
  INNER JOIN TECNICO.ESTRUTURA_CURRICULAR_TECNICA ECT ON ECT.ID_CURSO=D.ID_CURSO
WHERE C.ID_UNIDADE=$P{unidade} -- 284 = MUSICA
  AND C.NIVEL='T'
  AND D.STATUS=1 -- ATIVO
) A  
WHERE SEM_MATRICULA='X' OR REPROVADO_3_VEZES='X' 
   OR REPROVACOES_4_VEZES='X' OR FALTOU_DMAIS='X' OR EXCEDEU_PRAZO='X'
ORDER BY CURSO,ESPECIALIZACAO,NOME]]></queryString>

