	<queryString><![CDATA[SELECT * FROM (
SELECT DISTINCT U.SIGLA AS CENTRO,C.NOME AS CURSO,COALESCE(H.NOME,' ') AS HABILITACAO,G.DESCRICAO AS GRAU_ACADEMICO, 
        ME.DESCRICAO AS MODALIDADE,M.NOME AS CIDADE, T.SIGLA,
        CUR.CODIGO, CUR.SEMESTRE_CONCLUSAO_IDEAL AS NIVEL,CUR.CH_TOTAL_MINIMA,CH_OPTATIVAS_MINIMA,
        (SELECT SUM(CD.ch_Aula + CD.ch_Laboratorio + CD.ch_Estagio)
           FROM GRADUACAO.CURRICULO_COMPONENTE CC
          INNER JOIN ensino.COMPONENTE_CURRICULAR C ON C.ID_DISCIPLINA=CC.ID_COMPONENTE_CURRICULAR
          INNER JOIN ENSINO.componente_curricular_detalhes CD ON CD.ID_COMPONENTE_DETALHES=C.ID_DETALHE
          WHERE CC.ID_CURRICULO = CUR.ID_CURRICULO
           AND CC.obrigatoria = true
        ) AS CH_obrigatoria
  FROM GRADUACAO.CURRICULO CUR
 INNER JOIN GRADUACAO.MATRIZ_CURRICULAR MC ON MC.ID_MATRIZ_CURRICULAR=CUR.ID_MATRIZ 
 LEFT JOIN  GRADUACAO.HABILITACAO        H ON H.ID_HABILITACAO=MC.ID_HABILITACAO
 INNER JOIN PUBLIC.CURSO                 C ON C.ID_CURSO=CUR.ID_CURSO
 INNER JOIN COMUM.UNIDADE               U ON U.ID_UNIDADE=C.ID_UNIDADE
 LEFT JOIN  ENSINO.TURNO                 T ON T.ID_TURNO=MC.ID_TURNO
 LEFT JOIN  COMUM.MODALIDADE_EDUCACAO  ME ON ME.ID_MODALIDADE_EDUCACAO=C.ID_MODALIDADE_EDUCACAO
 LEFT JOIN  COMUM.MUNICIPIO             M ON M.ID_MUNICIPIO=C.ID_MUNICIPIO
 LEFT JOIN  ENSINO.GRAU_ACADEMICO        G ON G.ID_GRAU_ACADEMICO=MC.ID_GRAU_ACADEMICO
 WHERE ($P{unidade}=-1) OR (U.ID_UNIDADE=$P{unidade})
   AND CUR.ATIVO
   AND MC.ATIVO
   AND CUR.SITUACAO=2
 ORDER BY U.SIGLA,C.NOME) A
ORDER BY CENTRO,CURSO,HABILITACAO,GRAU_ACADEMICO]]></queryString>

