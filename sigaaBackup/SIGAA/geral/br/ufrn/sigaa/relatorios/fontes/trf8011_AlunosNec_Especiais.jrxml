	<queryString><![CDATA[SELECT T.DESCRICAO, D.MATRICULA, P.NOME, C.NOME AS CURSO_NOME
FROM  COMUM.PESSOA P
 INNER JOIN COMUM.PESSOA_NECESSIDADE_ESPECIAL PNEE ON (PNEE.id_pessoa = P.ID_PESSOA) 
 LEFT JOIN COMUM.TIPO_NECESSIDADE_ESPECIAL T ON T.ID_TIPO_NECESSIDADE_ESPECIAL = PNEE.ID_NECESSIDADE_ESPECIAL
 INNER JOIN PUBLIC.DISCENTE  D ON P.ID_PESSOA=D.ID_PESSOA
 LEFT  JOIN PUBLIC.CURSO     C ON C.ID_CURSO=D.ID_CURSO
WHERE D.STATUS IN (1,2,8,9,14) -- STATUS ATIVOS E COM VÍNCULO
   AND ($P{tipo} = -1 OR PNEE.ID_NECESSIDADE_ESPECIAL = $P{tipo})
   AND P.ID_PESSOA IN ( SELECT P.ID_PESSOA
			FROM PUBLIC.DISCENTE D
			 INNER JOIN COMUM.PESSOA   P ON P.ID_PESSOA = D.ID_PESSOA
			 INNER JOIN COMUM.PESSOA_NECESSIDADE_ESPECIAL PNEE ON (PNEE.id_pessoa = P.ID_PESSOA) 
			 INNER JOIN COMUM.TIPO_NECESSIDADE_ESPECIAL T ON T.ID_TIPO_NECESSIDADE_ESPECIAL = PNEE.ID_NECESSIDADE_ESPECIAL
			WHERE D.STATUS IN (1,2,8,9,14) -- ATIVOS
			GROUP BY p.id_PESSOA, P.NOME
			HAVING COUNT(*) = 1
			ORDER BY p.id_PESSOA DESC )

UNION 

SELECT T.DESCRICAO, D.MATRICULA, P.NOME, C.NOME AS CURSO_NOME
FROM  COMUM.PESSOA P
 INNER JOIN COMUM.PESSOA_NECESSIDADE_ESPECIAL PNEE ON (PNEE.id_pessoa = P.ID_PESSOA)
 LEFT JOIN COMUM.TIPO_NECESSIDADE_ESPECIAL T ON T.ID_TIPO_NECESSIDADE_ESPECIAL = PNEE.ID_NECESSIDADE_ESPECIAL
 INNER JOIN PUBLIC.DISCENTE  D ON P.ID_PESSOA=D.ID_PESSOA
 LEFT  JOIN PUBLIC.CURSO     C ON C.ID_CURSO=D.ID_CURSO
WHERE D.STATUS IN (1,2,8,9,14) -- STATUS ATIVOS E COM VÍNCULO
   AND ($P{tipo} = -1 OR PNEE.ID_NECESSIDADE_ESPECIAL = $P{tipo})
   AND D.ID_DISCENTE IN ( SELECT MAX (d.id_discente)
			FROM PUBLIC.DISCENTE D
			 INNER JOIN COMUM.PESSOA   P ON P.ID_PESSOA = D.ID_PESSOA
			 INNER JOIN COMUM.PESSOA_NECESSIDADE_ESPECIAL PNEE ON (PNEE.id_pessoa = P.ID_PESSOA) 
			 INNER JOIN COMUM.TIPO_NECESSIDADE_ESPECIAL T ON T.ID_TIPO_NECESSIDADE_ESPECIAL = PNEE.ID_NECESSIDADE_ESPECIAL
			WHERE D.STATUS IN (1,2,8,9,14) -- ATIVOS
			GROUP BY p.id_PESSOA, P.NOME
			HAVING COUNT(*) > 1
			ORDER BY p.id_PESSOA DESC )
			
ORDER BY DESCRICAO,NOME]]></queryString>

