	<queryString><![CDATA[SELECT 'PERMANENTE' AS QUADRO,DESCRICAO,MINIMO,MASCULINO,FEMININO,MASCULINO+FEMININO AS TOTAL
  FROM (
SELECT A.DESCRICAO,
       (
        SELECT COUNT(*)
		  FROM FUNCIONAL.HISTORICO_SERVIDOR H
         INNER JOIN RH.SERVIDOR S ON S.ID_SERVIDOR=H.ID_SERVIDOR
		 INNER JOIN COMUM.PESSOA  P ON P.ID_PESSOA=S.ID_PESSOA
		 LEFT  JOIN COMUM.UNIDADE U ON U.ID_UNIDADE=S.ID_UNIDADE
		 WHERE H.ANO=$P{ano} AND H.MES=$P{mes}
           AND S.ID_CATEGORIA=1 /*DOCENTE*/
		   AND S.ID_SITUACAO IN (1,20) /*ATIVO/CELETISTA*/
           AND P.SEXO='M'
           AND date_part('YEAR', age(cast($P{ano}||'-'||$P{mes}||'-01' as timestamp), P.DATA_NASCIMENTO)) BETWEEN A.MINIMO AND A.MAXIMO
           AND S.ID_SERVIDOR NOT IN (SELECT ID_SERVIDOR 
                                       FROM FUNCIONAL.OCORRENCIA_FUNCIONAL OC
 		          					   LEFT JOIN FUNCIONAL.OCORRENCIA O ON O.ID_OCORRENCIA=OC.ID_OCORRENCIA
									LEFT JOIN FUNCIONAL.ASSOCIACAO_GRUPO_OCORRENCIA AGO ON O.ID_OCORRENCIA=AGO.ID_OCORRENCIA
					    		      WHERE OC.ID_SERVIDOR=S.ID_SERVIDOR 
					 		            AND AGO.ID_GRUPO_OCORRENCIA=2
                                        AND EXTRACT(YEAR FROM OC.DATA)<=$P{ano} 
                                        AND EXTRACT(MONTH FROM OC.DATA)<=$P{mes})
       ) AS MASCULINO,
       (
        SELECT COUNT(*)
		  FROM FUNCIONAL.HISTORICO_SERVIDOR H
         INNER JOIN RH.SERVIDOR S ON S.ID_SERVIDOR=H.ID_SERVIDOR
		 INNER JOIN COMUM.PESSOA  P ON P.ID_PESSOA=S.ID_PESSOA
		 LEFT  JOIN COMUM.UNIDADE U ON U.ID_UNIDADE=S.ID_UNIDADE
		 WHERE H.ANO=$P{ano} AND H.MES=$P{mes}
           AND S.ID_CATEGORIA=1 /*DOCENTE*/
		   AND S.ID_SITUACAO IN (1,20) /*ATIVO/CELETISTA*/
           AND P.SEXO='F'
           AND date_part('YEAR', age(cast($P{ano}||'-'||$P{mes}||'-01' as timestamp), P.DATA_NASCIMENTO)) BETWEEN A.MINIMO AND A.MAXIMO
           AND S.ID_SERVIDOR NOT IN (SELECT ID_SERVIDOR 
                                       FROM FUNCIONAL.OCORRENCIA_FUNCIONAL OC
 		          					   LEFT JOIN FUNCIONAL.OCORRENCIA O ON O.ID_OCORRENCIA=OC.ID_OCORRENCIA
								LEFT JOIN FUNCIONAL.ASSOCIACAO_GRUPO_OCORRENCIA AGO ON O.ID_OCORRENCIA=AGO.ID_OCORRENCIA
					    		      WHERE OC.ID_SERVIDOR=S.ID_SERVIDOR 
					 		            AND AGO.ID_GRUPO_OCORRENCIA=2
                                        AND EXTRACT(YEAR FROM OC.DATA)<=$P{ano} 
                                        AND EXTRACT(MONTH FROM OC.DATA)<=$P{mes})
       ) AS FEMININO,MINIMO
FROM (
SELECT 'MENOR QUE 24 ANOS' AS DESCRICAO,0 AS MINIMO,24 AS MAXIMO
UNION
SELECT 'DE 25 A 29 ANOS',25,29
UNION 
SELECT 'DE 30 A 34 ANOS',30,34
UNION 
SELECT 'DE 35 A 39 ANOS',35,39
UNION 
SELECT 'DE 40 A 44 ANOS',40,44
UNION 
SELECT 'DE 45 A 49 ANOS',45,49
UNION 
SELECT 'DE 50 A 54 ANOS',50,54
UNION 
SELECT 'DE 55 A 59 ANOS',55,59
UNION 
SELECT 'DE 60 A 64 ANOS',60,64
UNION 
SELECT '65 ANOS OU MAIS',65,999
) A
ORDER BY A.MINIMO
) B
-----------------------------------------------------------------------
UNION
-----------------------------------------------------------------------
SELECT 'TEMPORÁRIO' AS QUADRO,DESCRICAO,MINIMO,MASCULINO,FEMININO,MASCULINO+FEMININO AS TOTAL
  FROM (
SELECT A.DESCRICAO,(
      SELECT count(DISTINCT ID_SERVIDOR)
          FROM (SELECT s.id_servidor,
                      (SELECT AGO.ID_GRUPO_OCORRENCIA
                         FROM FUNCIONAL.OCORRENCIA_FUNCIONAL OC
                         LEFT JOIN FUNCIONAL.OCORRENCIA O ON O.ID_OCORRENCIA=OC.ID_OCORRENCIA
			LEFT JOIN FUNCIONAL.ASSOCIACAO_GRUPO_OCORRENCIA AGO ON O.ID_OCORRENCIA=AGO.ID_OCORRENCIA
                        WHERE OC.ID_SERVIDOR=S.ID_SERVIDOR
                          AND AGO.ID_GRUPO_OCORRENCIA=2
                          AND EXTRACT(YEAR FROM OC.DATA)<=$P{ano} 
                          AND EXTRACT(MONTH FROM OC.DATA)<=$P{mes} LIMIT 1
                      ) AS ID_GRUPO_OCORRENCIA
   				  FROM FUNCIONAL.HISTORICO_SERVIDOR H
        		 INNER JOIN RH.SERVIDOR 					S ON S.ID_SERVIDOR=H.ID_SERVIDOR
                 INNER JOIN RH.CARGO             			C ON C.ID=S.ID_CARGO
                 INNER JOIN COMUM.PESSOA        			P ON P.ID_PESSOA=S.ID_PESSOA
                 INNER JOIN COMUM.UNIDADE       			U ON U.ID_UNIDADE=S.ID_UNIDADE
                  LEFT JOIN RH.SITUACAO_SERVIDOR 		   SS ON SS.ID_SITUACAO=S.ID_SITUACAO
                 WHERE S.ID_CATEGORIA=1 /*DOCENTE*/
                   AND S.ID_SITUACAO IN (1,12,19,20)
                   AND S.ID_ATIVO=1
                   AND S.ID_CARGO IN (/*60001,*/60002,60003,
                                      8036,8028,8010,8044,8230,8222,8214,8249
                                     )
                   AND P.SEXO='M'
                   AND date_part('YEAR', age(cast($P{ano}||'-'||$P{mes}||'-01' as timestamp), P.DATA_NASCIMENTO)) BETWEEN A.MINIMO AND A.MAXIMO
                 ORDER BY C.DENOMINACAO,P.NOME
               ) A
         WHERE ((A.ID_GRUPO_OCORRENCIA<>2) OR (A.ID_GRUPO_OCORRENCIA IS NULL))
           AND (SELECT SUM(VALOR_NO_MES)
                  FROM (
                        select case
                                  when $P{mes}-1 =0 THEN (SELECT DEZEMBRO
                                                                              FROM financeiro.ficha_financeira
                                                                             WHERE ID_SERVIDOR=A.ID_SERVIDOR
                                                                               AND ANO=$P{ano}-1
                                                                               AND ID_RUBRICA=7422 LIMIT 1
                                                                           )
                                  when $P{mes}-1 =1 THEN FF.JANEIRO
                                  when $P{mes}-1 =2 THEN FF.FEVEREIRO
                                  when $P{mes}-1 =3 THEN FF.MARCO
                                  when $P{mes}-1 =4 THEN FF.ABRIL
                                  when $P{mes}-1 =5 THEN FF.MAIO
                                  when $P{mes}-1 =6 THEN FF.JUNHO
                                  when $P{mes}-1 =7 THEN FF.JULHO
                                  when $P{mes}-1 =8 THEN FF.AGOSTO
                                  when $P{mes}-1 =9 THEN FF.SETEMBRO
                                  when $P{mes}-1 =10 THEN FF.OUTUBRO
                                  when $P{mes}-1 =11 THEN FF.NOVEMBRO
                               END AS VALOR_NO_MES
                          from financeiro.ficha_financeira FF
                         where id_servidor=A.ID_SERVIDOR
                           and ano =$P{ano}
                           and id_rubrica = 7422
                         LIMIT 1
                       ) A
               )>0
   ) AS MASCULINO,
  (
      SELECT count(DISTINCT ID_SERVIDOR)
          FROM (SELECT s.id_servidor,
                      (SELECT AGO.ID_GRUPO_OCORRENCIA
                         FROM FUNCIONAL.OCORRENCIA_FUNCIONAL OC
                         LEFT JOIN FUNCIONAL.OCORRENCIA O ON O.ID_OCORRENCIA=OC.ID_OCORRENCIA
			LEFT JOIN FUNCIONAL.ASSOCIACAO_GRUPO_OCORRENCIA AGO ON O.ID_OCORRENCIA=AGO.ID_OCORRENCIA
                        WHERE OC.ID_SERVIDOR=S.ID_SERVIDOR
                          AND AGO.ID_GRUPO_OCORRENCIA=2
                          AND EXTRACT(YEAR FROM OC.DATA)<=$P{ano} 
                          AND EXTRACT(MONTH FROM OC.DATA)<=$P{mes} LIMIT 1
                      ) AS ID_GRUPO_OCORRENCIA
   				  FROM FUNCIONAL.HISTORICO_SERVIDOR H
        		 INNER JOIN RH.SERVIDOR 					S ON S.ID_SERVIDOR=H.ID_SERVIDOR
                 INNER JOIN RH.CARGO             			C ON C.ID=S.ID_CARGO
                 INNER JOIN COMUM.PESSOA        			P ON P.ID_PESSOA=S.ID_PESSOA
                 INNER JOIN COMUM.UNIDADE       			U ON U.ID_UNIDADE=S.ID_UNIDADE
                  LEFT JOIN RH.SITUACAO_SERVIDOR 		   SS ON SS.ID_SITUACAO=S.ID_SITUACAO
                 WHERE S.ID_CATEGORIA=1 /*DOCENTE*/
                   AND S.ID_SITUACAO IN (1,12,19,20,54,52,53)
		   AND S.ADMISSAO < CAST($P{ano}||'-'||$P{mes}||'-01' as timestamp) and (S.data_desligamento is null or S.data_desligamento > CAST($P{ano}||'-'||$P{mes}||'-01' as timestamp))
                   AND S.ID_CARGO IN (/*60001,*/60002,60003,
                                      8036,8028,8010,8044,8230,8222,8214,8249
                                     )
                   AND P.SEXO='F'
                   AND date_part('YEAR', age(cast($P{ano}||'-'||$P{mes}||'-01' as timestamp), P.DATA_NASCIMENTO)) BETWEEN A.MINIMO AND A.MAXIMO
                 ORDER BY C.DENOMINACAO,P.NOME
               ) A
         WHERE ((A.ID_GRUPO_OCORRENCIA<>2) OR (A.ID_GRUPO_OCORRENCIA IS NULL))
           AND (SELECT SUM(VALOR_NO_MES)
                  FROM (
                        select case
                                  when $P{mes}-1 =0 THEN (SELECT DEZEMBRO
                                                                              FROM financeiro.ficha_financeira
                                                                             WHERE ID_SERVIDOR=A.ID_SERVIDOR
                                                                               AND ANO=$P{ano}-1
                                                                               AND ID_RUBRICA=7422 LIMIT 1
                                                                           )
                                  when $P{mes}-1 =1 THEN FF.JANEIRO
                                  when $P{mes}-1 =2 THEN FF.FEVEREIRO
                                  when $P{mes}-1 =3 THEN FF.MARCO
                                  when $P{mes}-1 =4 THEN FF.ABRIL
                                  when $P{mes}-1 =5 THEN FF.MAIO
                                  when $P{mes}-1 =6 THEN FF.JUNHO
                                  when $P{mes}-1 =7 THEN FF.JULHO
                                  when $P{mes}-1 =8 THEN FF.AGOSTO
                                  when $P{mes}-1 =9 THEN FF.SETEMBRO
                                  when $P{mes}-1 =10 THEN FF.OUTUBRO
                                  when $P{mes}-1 =11 THEN FF.NOVEMBRO
                               END AS VALOR_NO_MES
                          from financeiro.ficha_financeira FF
                         where id_servidor=A.ID_SERVIDOR
                           and ano =$P{ano}
                           and id_rubrica = 7422
                         LIMIT 1
                       ) A
               )>0
       ) AS FEMININO,MINIMO
FROM (
SELECT 'MENOR QUE 24 ANOS' AS DESCRICAO,0 AS MINIMO,24 AS MAXIMO
UNION
SELECT 'DE 25 A 29 ANOS',25,29
UNION 
SELECT 'DE 30 A 34 ANOS',30,34
UNION 
SELECT 'DE 35 A 39 ANOS',35,39
UNION 
SELECT 'DE 40 A 44 ANOS',40,44
UNION 
SELECT 'DE 45 A 49 ANOS',45,49
UNION 
SELECT 'DE 50 A 54 ANOS',50,54
UNION 
SELECT 'DE 55 A 59 ANOS',55,59
UNION 
SELECT 'DE 60 A 64 ANOS',60,64
UNION 
SELECT '65 ANOS OU MAIS',65,999
) A
ORDER BY A.MINIMO   
) B
ORDER BY QUADRO,MINIMO]]></queryString>

