	<queryString><![CDATA[select ps.nome as concurso,
coalesce(curso.nome||coalesce(' - '||grau_academico.descricao,'')||coalesce(' - '||turno.sigla,'')
||coalesce(' - '||habilitacao.nome,'')||' - '||municipio.nome,'SEM RESERVAS') as cursoCompleto, 
d.periodo_ingresso as semestreAprovacao, p.nome, p.cpf_cnpj as cpf,
inscricao_vestibular.numero_inscricao as numeroInscricao,
municipio.nome as municipioCurso,
gcvc.descricao as grupoCota,
cpsd.dentro_numero_vagas as dentroNumeroVagas
from graduacao.discente_graduacao dg
join discente d on dg.id_discente_graduacao = d.id_discente
inner join comum.pessoa p using (id_pessoa)
join graduacao.matriz_curricular using (id_matriz_curricular)
left join ensino.turno on (turno.id_turno = matriz_curricular.id_turno)
left join graduacao.habilitacao using (id_habilitacao)
left join ensino.grau_academico using (id_grau_academico)
left join curso on (matriz_curricular.id_curso = curso.id_curso)
left join comum.municipio using (id_municipio)
inner join vestibular.convocacao_processo_seletivo_discente cpsd using (id_discente)
inner join vestibular.convocacao_processo_seletivo cps using (id_convocacao_processo_seletivo)
inner join vestibular.processo_seletivo ps using (id_processo_seletivo)
inner join vestibular.inscricao_vestibular using (id_inscricao_vestibular)
left join ensino.grupo_cota_vaga_curso gcvc using (id_grupo_cota_vaga_curso)
where d.status in ( 1,2,5,8,9 ) 
and ps.nome ilike $P{nomeProcessoSeletivo}
and cps.descricao ilike $P{descricaoConvocacao}
order by municipioCurso, cursoCompleto, gcvc.descricao, p.nome_ascii]]></queryString>

