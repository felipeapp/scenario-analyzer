	<parameter name="mes" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(2)]]></defaultValueExpression>
	</parameter>
	<parameter name="ano" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(2008)]]></defaultValueExpression>
	</parameter>
	<parameter name="centro" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(-1)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT *,A.DOCENTES+A.TECNICOS AS TOTAL
 FROM (
      SELECT DISTINCT ID_UNIDADE,CENTRO,ID_DEPTO,NOME,
               ( SELECT count(*)
                    FROM FUNCIONAL.HISTORICO_SERVIDOR H2
                   INNER JOIN RH.SERVIDOR S2 ON S2.ID_SERVIDOR=H2.ID_SERVIDOR
                    LEFT JOIN COMUM.UNIDADE U_1 ON U_1.ID_UNIDADE=S2.ID_UNIDADE
                    LEFT JOIN COMUM.UNIDADE U_2 ON U_2.ID_UNIDADE=U_1.UNIDADE_RESPONSAVEL
                   WHERE H2.ANO=$P{ano} AND H2.MES=$P{mes}
                     AND S2.ID_UNIDADE=A.ID_DEPTO
                     AND S2.ID_CATEGORIA=1 /*DOCENTE*/
                     AND S2.ID_SITUACAO IN (1,20) /*ATIVO/CELETISTA*/
                     AND NOT EXISTS (SELECT ID_SERVIDOR 
                                                 FROM FUNCIONAL.OCORRENCIA_FUNCIONAL OC
                                                 LEFT JOIN FUNCIONAL.OCORRENCIA O ON O.ID_OCORRENCIA=OC.ID_OCORRENCIA
						LEFT JOIN FUNCIONAL.ASSOCIACAO_GRUPO_OCORRENCIA AGO ON O.ID_OCORRENCIA=AGO.ID_OCORRENCIA
                                                WHERE OC.ID_SERVIDOR=S2.ID_SERVIDOR 
                                                  AND AGO.ID_GRUPO_OCORRENCIA=2
                                                  AND EXTRACT(YEAR FROM OC.DATA)<=$P{ano} 
                                                  AND EXTRACT(MONTH FROM OC.DATA)<=$P{mes})
               ) AS DOCENTES,
               ( SELECT count(*)
                    FROM FUNCIONAL.HISTORICO_SERVIDOR H2
                   INNER JOIN RH.SERVIDOR S2 ON S2.ID_SERVIDOR=H2.ID_SERVIDOR
                    LEFT JOIN COMUM.UNIDADE U_1 ON U_1.ID_UNIDADE=S2.ID_UNIDADE
                    LEFT JOIN COMUM.UNIDADE U_2 ON U_2.ID_UNIDADE=U_1.UNIDADE_RESPONSAVEL
                   WHERE H2.ANO=$P{ano} AND H2.MES=$P{mes}
                     AND S2.ID_UNIDADE=A.ID_DEPTO
                     AND S2.ID_CATEGORIA=2 /*TECNICOS*/
                     AND S2.ID_SITUACAO IN (1,20) /*ATIVO/CELETISTA*/
		     AND S2.ID_CLASSE_FUNCIONAL NOT IN (1,2) /* SUJEIRA */
                     AND NOT EXISTS (SELECT ID_SERVIDOR 
                                                 FROM FUNCIONAL.OCORRENCIA_FUNCIONAL OC
                                                 LEFT JOIN FUNCIONAL.OCORRENCIA O ON O.ID_OCORRENCIA=OC.ID_OCORRENCIA
						LEFT JOIN FUNCIONAL.ASSOCIACAO_GRUPO_OCORRENCIA AGO ON O.ID_OCORRENCIA=AGO.ID_OCORRENCIA
                                                WHERE OC.ID_SERVIDOR=S2.ID_SERVIDOR 
                                                  AND AGO.ID_GRUPO_OCORRENCIA=2
                                                  AND EXTRACT(YEAR FROM OC.DATA)<=$P{ano} 
                                                  AND EXTRACT(MONTH FROM OC.DATA)<=$P{mes})
               ) AS TECNICOS
        FROM (  
              SELECT U2.ID_UNIDADE,U2.NOME AS CENTRO,U.ID_UNIDADE AS ID_DEPTO,U.NOME
                FROM FUNCIONAL.HISTORICO_SERVIDOR H
               INNER JOIN RH.SERVIDOR S ON S.ID_SERVIDOR=H.ID_SERVIDOR
                LEFT JOIN COMUM.UNIDADE U ON U.ID_UNIDADE=S.ID_UNIDADE
                LEFT JOIN COMUM.UNIDADE U2 ON U2.ID_UNIDADE=U.UNIDADE_RESPONSAVEL
               WHERE H.ANO=$P{ano} AND H.MES=$P{mes}
                 AND S.ID_CATEGORIA IN (1,2)
                 AND S.ID_SITUACAO IN (1,20) /*ATIVO/CELETISTA*/
                 AND NOT EXISTS (SELECT ID_SERVIDOR 
                                             FROM FUNCIONAL.OCORRENCIA_FUNCIONAL OC
                                             LEFT JOIN FUNCIONAL.OCORRENCIA O ON O.ID_OCORRENCIA=OC.ID_OCORRENCIA
						LEFT JOIN FUNCIONAL.ASSOCIACAO_GRUPO_OCORRENCIA AGO ON O.ID_OCORRENCIA=AGO.ID_OCORRENCIA
                                            WHERE OC.ID_SERVIDOR=S.ID_SERVIDOR 
                                              AND AGO.ID_GRUPO_OCORRENCIA=2
                                              AND EXTRACT(YEAR FROM OC.DATA)<=$P{ano} 
                                              AND EXTRACT(MONTH FROM OC.DATA)<=$P{mes})
              ORDER BY U2.NOME,U.NOME
             ) A
      WHERE (ID_UNIDADE=$P{centro} OR $P{centro}=-1)
      ORDER BY CENTRO,NOME
) A
WHERE ID_UNIDADE IS NOT NULL]]></queryString>

