	</parameter>
	<queryString><![CDATA[SELECT QUALIFICACAO,CESSAO,SAUDE,QUALIFICACAO+CESSAO+SAUDE AS TOTAL FROM (
SELECT 
(SELECT COUNT(*)
  FROM FUNCIONAL.AUSENCIA A
 INNER JOIN RH.SERVIDOR S ON S.ID_SERVIDOR=A.ID_SERVIDOR
 WHERE A.ID_OCORRENCIA IN (31622,31632,31702,31712,31722,31733,180381,180391,180401,301634,301704,
                           301714,302594,302604,302614,302624,302634,302644,302654)
   AND S.ID_UNIDADE=$P{unidade}
   AND S.ID_CATEGORIA = 1
   AND (
        (EXTRACT(YEAR FROM CAST('NOW' AS DATE))>=EXTRACT(YEAR FROM A.INICIO)) 
       AND 
        (EXTRACT(YEAR FROM CAST('NOW' AS DATE))<=EXTRACT(YEAR FROM A.FIM))
       )
) AS QUALIFICACAO,
(SELECT COUNT(*)
  FROM FUNCIONAL.AUSENCIA A
 INNER JOIN RH.SERVIDOR S ON S.ID_SERVIDOR=A.ID_SERVIDOR
 WHERE A.ID_OCORRENCIA IN (10142,31642,31652,301034,301044)
   AND S.ID_UNIDADE=$P{unidade}
   AND S.ID_CATEGORIA = 1
   AND (
        (EXTRACT(YEAR FROM CAST('NOW' AS DATE))>=EXTRACT(YEAR FROM A.INICIO)) 
       AND 
        (EXTRACT(YEAR FROM CAST('NOW' AS DATE))<=EXTRACT(YEAR FROM A.FIM))
       )
) AS CESSAO,
(SELECT COUNT(*)
  FROM FUNCIONAL.AUSENCIA A
 INNER JOIN RH.SERVIDOR S ON S.ID_SERVIDOR=A.ID_SERVIDOR
 WHERE A.ID_OCORRENCIA IN (30382,30442,30452,30672,31002,31132,31142,31332,31592,31612
						  ,39992,40132,40152,40162,40182,40192,40252,180261,180271,180281
						  ,180441,180621,180641,180811,180831,180841,180921,180931,182511
						  ,182531,301144,301244,301594,302584,400104)
   AND S.ID_UNIDADE=$P{unidade}
   AND S.ID_CATEGORIA = 1
   AND (
        (EXTRACT(YEAR FROM CAST('NOW' AS DATE))>=EXTRACT(YEAR FROM A.INICIO)) 
       AND 
        (EXTRACT(YEAR FROM CAST('NOW' AS DATE))<=EXTRACT(YEAR FROM A.FIM))
       )
) AS SAUDE
FROM RH.SERVIDOR LIMIT 1
) A]]></queryString>

