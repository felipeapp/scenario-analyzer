	<queryString><![CDATA[SELECT U.ID_UNIDADE,U.NOME,
      (SELECT MAX(S.ADMISSAO) 
	 FROM RH.SERVIDOR S 
	WHERE S.ID_UNIDADE IN (
		SELECT COALESCE (unid2.id_unidade, u.id_unidade) 
		FROM comum.unidade u
		LEFT JOIN comum.unidade unid2 on ( u.id_nivel_organizacional = 2 and u.unidade_responsavel = unid2.unidade_responsavel 
			and unid2.organizacional and unid2.tipo_academica = 1 and unid2.id_nivel_organizacional = 3)
		WHERE u.id_unidade = $P{unidade}
             )
          AND S.ID_CATEGORIA=1
	  AND ID_TIPO_REGIME_JURIDICO IN (1,2,3) 
	  AND (ID_SITUACAO IN (1,20) /*ATIVO/CELETISTA*/)
       ) AS DATA_ULTIMA_ADMISSAO
 FROM COMUM.UNIDADE U WHERE ID_UNIDADE IN (
		SELECT COALESCE (unid2.id_unidade, u.id_unidade) 
		FROM comum.unidade u
		LEFT JOIN comum.unidade unid2 on ( u.id_nivel_organizacional = 2 and u.unidade_responsavel = unid2.unidade_responsavel 
			and unid2.organizacional and unid2.tipo_academica = 1 and unid2.id_nivel_organizacional = 3)
		WHERE u.id_unidade = $P{unidade}
	)]]></queryString>

