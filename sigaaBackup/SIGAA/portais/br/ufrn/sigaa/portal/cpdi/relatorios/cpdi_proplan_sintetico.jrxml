	<parameter name="unidade" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(122)]]></defaultValueExpression>
	</parameter>
	<parameter name="ano" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(2007)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT DISTINCT U.ID_UNIDADE AS ID_UNIDADE,U.NOME AS UNIDADE_NOME,PE.ID_PESSOA,S.ID_SERVIDOR,PE.NOME,substr(c.denominacao,11,50) as DENOMINACAO,
                CASE S.ID_ATIVO
                    WHEN 1 THEN 'ATIVO'
                    ELSE 'CEDIDO'
                END AS SITUACAO,
                CASE S.REGIME_TRABALHO
                   WHEN 99 THEN 'DE'
                   ELSE CAST(S.REGIME_TRABALHO AS CHAR(2))
                END AS REGIME,
                (SELECT COUNT(DISTINCT T.ID_DISCIPLINA) FROM ENSINO.TURMA T 
                  INNER JOIN ENSINO.DOCENTE_TURMA D ON D.ID_TURMA=T.ID_TURMA
                  INNER JOIN ENSINO.COMPONENTE_CURRICULAR CC ON CC.ID_DISCIPLINA= T.ID_DISCIPLINA
                  WHERE T.ANO = $P{ano}
                    AND D.ID_DOCENTE=S.ID_SERVIDOR
                    AND T.ID_SITUACAO_TURMA NOT IN(4,5)
                    AND CC.NIVEL='G'
		    AND ((T.ID_POLO IS NULL) OR T.ID_TURMA IN (SELECT RC.ID_TURMA
						    		 FROM GRADUACAO.reserva_curso RC
								INNER JOIN PUBLIC.CURSO C ON C.ID_CURSO=RC.ID_CURSO
								WHERE C.ID_MODALIDADE_EDUCACAO=2)  -- ENSINO A DISTANCIA
                        )
                ) AS GRADUACAO_QTD_DISCIPLINAS,
                (SELECT COUNT(*) FROM ENSINO.TURMA T 
                  INNER JOIN ENSINO.DOCENTE_TURMA D ON D.ID_TURMA=T.ID_TURMA
                  INNER JOIN ENSINO.COMPONENTE_CURRICULAR CC ON CC.ID_DISCIPLINA= T.ID_DISCIPLINA
                  WHERE T.ANO = $P{ano}
                    AND D.ID_DOCENTE=S.ID_SERVIDOR
                    AND T.ID_SITUACAO_TURMA NOT IN(4,5)
                    AND CC.NIVEL='G'
                    AND ((T.ID_POLO IS NULL) OR T.ID_TURMA IN (SELECT RC.ID_TURMA
						    		 FROM GRADUACAO.reserva_curso RC
								INNER JOIN PUBLIC.CURSO C ON C.ID_CURSO=RC.ID_CURSO
								WHERE C.ID_MODALIDADE_EDUCACAO=2)  -- ENSINO A DISTANCIA
                        )
                ) AS GRADUACAO_QTD_TURMAS,
                (SELECT SUM(D.CH_DEDICADA_PERIODO)/15 FROM ENSINO.TURMA T 
                  INNER JOIN ENSINO.DOCENTE_TURMA D ON D.ID_TURMA=T.ID_TURMA
                  INNER JOIN ENSINO.COMPONENTE_CURRICULAR CC ON CC.ID_DISCIPLINA= T.ID_DISCIPLINA
                  WHERE T.ANO = $P{ano}
                    AND D.ID_DOCENTE=S.ID_SERVIDOR
                    AND T.ID_SITUACAO_TURMA NOT IN(4,5)
                    AND CC.NIVEL='G'
                    AND ((T.ID_POLO IS NULL) OR T.ID_TURMA IN (SELECT RC.ID_TURMA
						    		 FROM GRADUACAO.reserva_curso RC
								INNER JOIN PUBLIC.CURSO C ON C.ID_CURSO=RC.ID_CURSO
								WHERE C.ID_MODALIDADE_EDUCACAO=2)  -- ENSINO A DISTANCIA
                        )
                ) AS GRADUACAO_QTD_CREDITOS,
                (SELECT COUNT(DISTINCT T.ID_DISCIPLINA) FROM ENSINO.TURMA T 
                  INNER JOIN ENSINO.DOCENTE_TURMA D ON D.ID_TURMA=T.ID_TURMA
                  INNER JOIN ENSINO.COMPONENTE_CURRICULAR CC ON CC.ID_DISCIPLINA= T.ID_DISCIPLINA
                  WHERE T.ANO = $P{ano}
                    AND D.ID_DOCENTE=S.ID_SERVIDOR
                    AND T.ID_SITUACAO_TURMA NOT IN(4,5)
                    AND CC.NIVEL='S'
                ) AS POS_QTD_DISCIPLINAS,
                (SELECT COUNT(*) FROM ENSINO.TURMA T 
                  INNER JOIN ENSINO.DOCENTE_TURMA D ON D.ID_TURMA=T.ID_TURMA
                  INNER JOIN ENSINO.COMPONENTE_CURRICULAR CC ON CC.ID_DISCIPLINA= T.ID_DISCIPLINA
                  WHERE T.ANO = $P{ano}
                    AND D.ID_DOCENTE=S.ID_SERVIDOR
                    AND T.ID_SITUACAO_TURMA NOT IN(4,5)
                    AND CC.NIVEL='S'
                ) AS POS_QTD_TURMAS,
                (SELECT SUM(CD.CR_AULA+CD.CR_LABORATORIO+CD.CR_ESTAGIO)
                   FROM ENSINO.TURMA T 
                  INNER JOIN ENSINO.DOCENTE_TURMA D ON D.ID_TURMA=T.ID_TURMA
                  INNER JOIN ENSINO.COMPONENTE_CURRICULAR CC ON CC.ID_DISCIPLINA= T.ID_DISCIPLINA
                  INNER JOIN ENSINO.COMPONENTE_CURRICULAR_DETALHES CD ON CD.ID_COMPONENTE_DETALHES=CC.ID_DETALHE
                  WHERE T.ANO = $P{ano}
                    AND D.ID_DOCENTE=S.ID_SERVIDOR
                    AND T.ID_SITUACAO_TURMA NOT IN(4,5)
                    AND CC.NIVEL='S'
                ) AS POS_QTD_CREDITOS,
                (SELECT TTF+MN AS QTD 
				   FROM (SELECT (select count(*) 
						           from prodocente.trabalho_fim_curso T 
						          where T.id_servidor=S.ID_SERVIDOR 
						            and EXTRACT(YEAR FROM T.data_defesa) = $P{ano}
						        ) AS TTF,
							    (select COUNT(*) 
						           from prodocente.monografia M
								  where M.id_servidor=S.ID_SERVIDOR
						            and M.id_tipo_orientacao=1 
						            and EXTRACT(YEAR FROM M.periodo_fim) = $P{ano}
						        ) AS MN
						   FROM PRODOCENTE.TRABALHO_FIM_CURSO LIMIT 1
					     ) A 
                ) AS ORIENTANDO_GRADUACAO,   
                (SELECT COUNT(*)
                   FROM PRODOCENTE.TESE_ORIENTADA T
                   LEFT JOIN PUBLIC.discente D ON D.ID_DISCENTE=T.ID_DISCENTE
                   LEFT JOIN COMUM.PESSOA P ON P.ID_PESSOA=D.ID_PESSOA
				  WHERE T.ID_SERVIDOR=S.ID_SERVIDOR           
					AND extract(year from t.periodo_inicio)<=$P{ano} 
					AND (T.periodo_fim is null or extract(year from T.periodo_fim)>=$P{ano}) 
					AND (T.data_publicacao is null or extract(year from T.data_publicacao)>$P{ano}) 
					AND T.id_tipo_orientacao=3 
					AND (T.ativo is null or T.ativo=true)
                ) AS ORIENTANDO_MESTRADO,   
                (SELECT COUNT(*)
                   FROM PRODOCENTE.TESE_ORIENTADA T
                  WHERE T.id_servidor=S.ID_SERVIDOR
					AND extract(year from T.periodo_inicio)<=$P{ano}
                    AND (T.periodo_fim is null or extract(year from T.periodo_fim)>=$P{ano}) 
                    AND (T.data_publicacao is null or extract(year from T.data_publicacao)>$P{ano})
                    AND T.id_tipo_orientacao=4 
                    AND (T.ativo is null or T.ativo=true)
                ) AS ORIENTANDO_DOUTORADO,   
                (SELECT COUNT(*)
				   FROM PRODOCENTE.ESTAGIO E 
				  WHERE  E.ID_SERVIDOR=S.ID_SERVIDOR
				    AND  EXTRACT(YEAR FROM PERIODO_INICIO)>= $P{ano}
				    AND  (EXTRACT(YEAR FROM PERIODO_FIM) <= $P{ano} OR PERIODO_FIM IS NULL)
    			) AS ORIENTACAO_ESTAGIO,
                (SELECT COUNT(DISTINCT PP.ID_PROJETO)
                    FROM PESQUISA.PROJETO_PESQUISA             PP
                   INNER JOIN PROJETOS.PROJETO                   PJ ON PJ.ID_PROJETO=PP.ID_PROJETO
                   INNER JOIN PROJETOS.MEMBRO_PROJETO          MP ON MP.ID_PROJETO_PESQUISA=PP.ID_PROJETO
                   INNER JOIN COMUM.PESSOA                    PE ON PE.ID_PESSOA=MP.ID_PESSOA
                   INNER JOIN RH.SERVIDOR                      S2 ON S2.ID_PESSOA=PE.ID_PESSOA
                   WHERE PP.COD_ANO=$P{ano}
                     AND S2.ID_SERVIDOR=S.ID_SERVIDOR
                     AND MP.TIPO_PARTICIPACAO=1
                ) AS PESQUISA_COORDENACAO,
                (SELECT COUNT(DISTINCT PP.ID_PROJETO)
                    FROM PESQUISA.PROJETO_PESQUISA             PP
                   INNER JOIN PROJETOS.PROJETO                   PJ ON PJ.ID_PROJETO=PP.ID_PROJETO
                   INNER JOIN PROJETOS.MEMBRO_PROJETO          MP ON MP.ID_PROJETO_PESQUISA=PP.ID_PROJETO
                   INNER JOIN COMUM.PESSOA                    PE ON PE.ID_PESSOA=MP.ID_PESSOA
                   INNER JOIN RH.SERVIDOR                      S2 ON S2.ID_PESSOA=PE.ID_PESSOA
                   WHERE PP.COD_ANO=$P{ano}
                     AND S2.ID_SERVIDOR=S.ID_SERVIDOR
                     AND MP.TIPO_PARTICIPACAO<>1
                ) AS PESQUISA_PARTICIPACAO,
                (
                SELECT COUNT(DISTINCT ID_DISCENTE)
                  FROM PESQUISA.MEMBRO_PROJETO_DISCENTE M
                  INNER JOIN PESQUISA.PLANO_TRABALHO P ON P.ID_PLANO_TRABALHO=M.ID_PLANO_TRABALHO
                  WHERE P.ID_PROJETO_PESQUISA IN (SELECT PP.ID_PROJETO
                                                    FROM PESQUISA.PROJETO_PESQUISA             PP
                                                   INNER JOIN PROJETOS.PROJETO                   PJ ON PJ.ID_PROJETO=PP.ID_PROJETO
                                                   INNER JOIN PROJETOS.MEMBRO_PROJETO          MP ON MP.ID_PROJETO_PESQUISA=PP.ID_PROJETO
                                                   INNER JOIN RH.SERVIDOR                      S2 ON S2.ID_PESSOA=MP.ID_PESSOA
                                                   WHERE PP.COD_ANO=$P{ano}
                                                     AND S2.ID_SERVIDOR=S.ID_SERVIDOR
                                                     AND MP.TIPO_PARTICIPACAO=1
                                                  )                     
                ) AS PESQUISA_BOLSISTAS,
                (SELECT COUNT(*)
                   FROM PRODOCENTE.TESE_ORIENTADA T
                  WHERE T.ID_SERVIDOR=S.ID_SERVIDOR
                    AND T.ID_DEPARTAMENTO=$P{unidade}
                    AND EXTRACT(YEAR FROM T.DATA_PUBLICACAO) =$P{ano}
                ) AS PESQUISA_TESESEDISSERTACOES,
				(SELECT COUNT(*) 
				   FROM EXTENSAO.ATIVIDADE                A
				  INNER JOIN PROJETOS.MEMBRO_PROJETO     MP ON MP.ID_ATIVIDADE=A.ID_ATIVIDADE
				  INNER JOIN PROJETOS.PROJETO               P ON P.ID_PROJETO=A.ID_ATIVIDADE
				  INNER JOIN PROJETOS.TIPO_SITUACAO_PROJETO T ON T.ID_TIPO_SITUACAO_PROJETO=P.ID_TIPO_SITUACAO_PROJETO
				  WHERE MP.ID_SERVIDOR=S.ID_SERVIDOR
				    AND  EXTRACT(YEAR FROM P.DATA_INICIO)>= $P{ano}
				    AND  (EXTRACT(YEAR FROM P.DATA_FIM) <= $P{ano} OR P.DATA_FIM IS NULL)
				    AND T.ID_TIPO_SITUACAO_PROJETO=103 -- APROVADO 
				    AND T.CONTEXTO='E' -- EXTENSAO 
				    AND MP.ID_FUNCAO_MEMBRO=1 -- COORDENADOR 
                ) AS EXTENSAO_COORDENACAO,    
		(SELECT count(*)
		  FROM EXTENSAO.ATIVIDADE                A
		  INNER JOIN PROJETOS.MEMBRO_PROJETO     MP ON MP.ID_ATIVIDADE=A.ID_ATIVIDADE
		  INNER JOIN PROJETOS.PROJETO               P ON P.ID_PROJETO=A.ID_ATIVIDADE
		  INNER JOIN PROJETOS.TIPO_SITUACAO_PROJETO T ON T.ID_TIPO_SITUACAO_PROJETO=P.ID_TIPO_SITUACAO_PROJETO
		  WHERE MP.ID_SERVIDOR=S.ID_SERVIDOR
		    AND  EXTRACT(YEAR FROM P.DATA_INICIO)>= $P{ano}
		    AND  (EXTRACT(YEAR FROM P.DATA_FIM) <= $P{ano} OR P.DATA_FIM IS NULL)
		    AND T.ID_TIPO_SITUACAO_PROJETO=103 -- APROVADO 
		    AND T.CONTEXTO='E' -- EXTENSAO 
		) AS EXTENSAO_PARTICIPA,
                (SELECT SUM(A.BOLSAS_CONCEDIDAS) AS BOLSAS_CONCEDIDAS
                            FROM EXTENSAO.ATIVIDADE A
                           INNER JOIN PROJETOS.PROJETO P ON P.ID_PROJETO=A.ID_ATIVIDADE
                           WHERE A.ID_ATIVIDADE IN (SELECT MP.ID_ATIVIDADE 
        	                                      FROM PROJETOS.MEMBRO_PROJETO MP
                	                             INNER JOIN RH.SERVIDOR        S2 ON S2.ID_PESSOA=MP.ID_PESSOA
                        	                     INNER JOIN PROJETOS.PROJETO     PJ ON PJ.ID_PROJETO=P.ID_PROJETO
                                	             WHERE PJ.ANO=$P{ano}
                                        	       AND S2.ID_SERVIDOR=S.ID_SERVIDOR
	                                               AND MP.ID_FUNCAO_MEMBRO=1
        	                                    )
                ) AS EXTENSAO_BOLSISTA,
                (SELECT COUNT(*)
                   FROM RH.DESIGNACAO  D 
                  INNER JOIN RH.ATIVIDADE   A        ON A.ID_ATIVIDADE=D.ID_ATIVIDADE
                  WHERE D.ID_SERVIDOR=S.ID_SERVIDOR
                  AND S.ID_SITUACAO IN (1,12,19,20)
                  AND ((D.FIM IS NULL) OR (D.FIM>CAST('NOW' AS DATE)))
                ) AS ATIVIDADES_ADMINISTRATIVAS,
		(SELECT COUNT(*) 
		   FROM PRODOCENTE.PRODUCAO 
		  WHERE ID_SERVIDOR=S.ID_SERVIDOR
		    AND ANO_REFERENCIA = $P{ano}
                ) AS PRODUCAO_INTELECTUAL_PONTUACAO    
  FROM RH.SERVIDOR S
 INNER JOIN RH.CARGO                   		 C  ON C.ID=S.ID_CARGO
 INNER JOIN COMUM.UNIDADE                   U  ON U.ID_UNIDADE=S.ID_UNIDADE 
 INNER JOIN COMUM.PESSOA                    PE ON PE.ID_PESSOA=S.ID_PESSOA
 WHERE S.ID_UNIDADE=$P{unidade}
  AND S.ID_CATEGORIA=1 -- DOCENTE
  AND S.ID_ATIVO IN (1,7)
 ORDER BY PE.NOME]]></queryString>

