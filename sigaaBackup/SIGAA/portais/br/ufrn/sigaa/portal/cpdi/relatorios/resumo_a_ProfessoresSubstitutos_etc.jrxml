	<parameter name="unidade" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(122)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT ID_UNIDADE,DENOMINACAO,TITULACAO,REGIME_TRABALHO,COUNT(*) AS QTD
  FROM (
SELECT ID_UNIDADE,DENOMINACAO,TITULACAO,
                        CASE WHEN REGIME_TRABALHO='20' THEN '20'
                            WHEN REGIME_TRABALHO='40' THEN '40'
                            WHEN REGIME_TRABALHO='99' THEN 'DE'
                            ELSE REGIME_TRABALHO
                        END AS REGIME_TRABALHO
  FROM ( 
	SELECT s.id_servidor,
           (SELECT O.ID_GRUPO_OCORRENCIA 
              FROM FUNCIONAL.OCORRENCIA_FUNCIONAL OC
              LEFT JOIN FUNCIONAL.OCORRENCIA O ON O.ID_OCORRENCIA=OC.ID_OCORRENCIA
             WHERE OC.ID_SERVIDOR=S.ID_SERVIDOR 
               AND O.ID_GRUPO_OCORRENCIA=2 LIMIT 1
           ) AS ID_GRUPO_OCORRENCIA
    ,S.ID_ATIVO,SS.DESCRICAO AS SITUACAO_DESCRICAO
    ,U.ID_UNIDADE,P.NOME,C.DENOMINACAO,F.DENOMINACAO AS TITULACAO,
		   CAST(S.REGIME_TRABALHO AS VARCHAR(2)) AS REGIME_TRABALHO,S.ADMISSAO
	  FROM RH.SERVIDOR S
	 INNER JOIN RH.CARGO             			C ON C.ID=S.ID_CARGO 
	 INNER JOIN COMUM.PESSOA        			P ON P.ID_PESSOA=S.ID_PESSOA
	 INNER JOIN COMUM.UNIDADE       			U ON U.ID_UNIDADE=S.ID_UNIDADE
	 LEFT  JOIN RH.FORMACAO          			F ON F.ID_FORMACAO=S.ID_FORMACAO
     LEFT  JOIN RH.SITUACAO_SERVIDOR 		   SS ON SS.ID_SITUACAO=S.ID_SITUACAO
	 WHERE S.ID_UNIDADE=$P{unidade}
	   AND S.ID_CATEGORIA=1 /*DOCENTE*/
	   AND S.ID_SITUACAO IN (1,12,19,20)
	   AND S.ID_CARGO IN (/*60001,*/60002,60003,
	                      8036,8028,8010,8044,8230,8222,8214,8249
	                     )
      ORDER BY C.DENOMINACAO,P.NOME 
       ) A
WHERE ((A.ID_GRUPO_OCORRENCIA<>2) OR (A.ID_GRUPO_OCORRENCIA IS NULL))
 ORDER BY DENOMINACAO,NOME
)A 
GROUP BY 1,2,3,4]]></queryString>

