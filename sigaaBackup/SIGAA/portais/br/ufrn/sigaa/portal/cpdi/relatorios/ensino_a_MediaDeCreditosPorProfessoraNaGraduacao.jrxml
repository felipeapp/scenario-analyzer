	<queryString><![CDATA[SELECT S.ID_SERVIDOR,P.NOME,
COALESCE(ROUND(CAST(SUM(CASE WHEN CC.NIVEL='T' AND T.ID_POLO IS NULL THEN DT.CH_DEDICADA_PERIODO ELSE NULL END) AS NUMERIC(8,1))
 / COUNT(CASE WHEN CC.NIVEL='T' AND T.ID_POLO IS NULL THEN DT.ID_DOCENTE_TURMA ELSE NULL END), 1), 0.0) AS CH_MEDIA_TECNICO,
COALESCE(ROUND(CAST(SUM(CASE WHEN CC.NIVEL='G' AND T.ID_POLO IS NULL THEN DT.CH_DEDICADA_PERIODO ELSE NULL END) AS NUMERIC(8,1))
 / COUNT(CASE WHEN CC.NIVEL='G' AND T.ID_POLO IS NULL THEN DT.ID_DOCENTE_TURMA ELSE NULL END), 1), 0.0) AS CH_MEDIA_GRADUACAO,
COALESCE(ROUND(CAST(SUM(CASE WHEN CC.NIVEL='S'  AND T.ID_POLO IS NULL THEN DT.CH_DEDICADA_PERIODO ELSE NULL END) AS NUMERIC(8,1))
 / COUNT(CASE WHEN CC.NIVEL='S' AND T.ID_POLO IS NULL THEN DT.ID_DOCENTE_TURMA ELSE NULL END), 1), 0.0) AS CH_MEDIA_STRICTO,
COALESCE(ROUND(CAST(SUM(CASE WHEN CC.NIVEL='L'  AND T.ID_POLO IS NULL THEN DT.CH_DEDICADA_PERIODO ELSE NULL END) AS NUMERIC(8,1))
 / COUNT(CASE WHEN CC.NIVEL='L' AND T.ID_POLO IS NULL THEN DT.ID_DOCENTE_TURMA ELSE NULL END), 1), 0.0) AS CH_MEDIA_LATO,
COALESCE(ROUND(CAST(SUM(CASE WHEN T.ID_POLO IS NOT NULL OR T.DISTANCIA = TRUE THEN CH_EAD.CH_DEDICADA ELSE NULL END) AS NUMERIC(8,1))
 / COUNT(CASE WHEN T.ID_POLO IS NOT NULL OR T.DISTANCIA = TRUE THEN CH_EAD.ID_CARGA_HORARIA_EAD ELSE NULL END), 1), 0.0) AS CH_MEDIA_EAD
FROM ENSINO.TURMA T
 INNER JOIN (
 	SELECT ID_TURMA FROM ENSINO.MATRICULA_COMPONENTE 
 	WHERE ANO = $P{ano} 
 	AND ($P{periodo}=-1 OR (($P{periodo}=1 and PERIODO IN (1,3)) OR ($P{periodo}=2 and PERIODO IN (2,4))))
 	GROUP BY ID_TURMA 
 	HAVING COUNT(ID_MATRICULA_COMPONENTE) > 0) TURMAS_COM_DISCENTES USING (ID_TURMA)
 INNER JOIN ENSINO.DOCENTE_TURMA DT ON DT.ID_TURMA=T.ID_TURMA
 LEFT JOIN ENSINO.DOCENTE_EXTERNO DE ON (DT.ID_DOCENTE_EXTERNO = DE.ID_DOCENTE_EXTERNO)
 INNER JOIN RH.SERVIDOR S ON (S.ID_SERVIDOR=DT.ID_DOCENTE OR DE.ID_SERVIDOR = S.ID_SERVIDOR)
 INNER JOIN COMUM.PESSOA P ON P.ID_PESSOA=S.ID_PESSOA
 INNER JOIN ENSINO.COMPONENTE_CURRICULAR CC ON CC.ID_DISCIPLINA=T.ID_DISCIPLINA
 INNER JOIN ENSINO.COMPONENTE_CURRICULAR_DETALHES CCD ON CCD.ID_COMPONENTE_DETALHES=CC.ID_DETALHE
 LEFT JOIN EAD.CARGA_HORARIA_EAD CH_EAD ON S.ID_SERVIDOR=CH_EAD.ID_SERVIDOR AND T.ANO = CH_EAD.ANO AND T.PERIODO = CH_EAD.PERIODO
 WHERE T.ANO=$P{ano}
   AND ($P{periodo}=-1 OR (($P{periodo}=1 and T.PERIODO IN (1,3)) OR ($P{periodo}=2 and T.PERIODO IN (2,4))))
   AND CC.NIVEL IN ('G','S','L','T') 
   AND S.ID_UNIDADE=$P{unidade}
 GROUP BY S.ID_SERVIDOR,P.NOME
 ORDER BY P.NOME]]></queryString>

