	<queryString><![CDATA[SELECT DISTINCT S.ID_UNIDADE,PP.COD_ANO AS ANO,D.MATRICULA,C.NOME AS CURSONOME,PE.NOME
  FROM PESQUISA.PROJETO_PESQUISA             PP
 INNER JOIN PESQUISA.PLANO_TRABALHO          P  ON P.ID_PROJETO_PESQUISA=PP.ID_PROJETO
 INNER JOIN PESQUISA.MEMBRO_PROJETO_DISCENTE M  ON M.ID_PLANO_TRABALHO=P.ID_PLANO_TRABALHO
 INNER JOIN RH.SERVIDOR                      S  ON S.ID_SERVIDOR=P.ID_ORIENTADOR
 INNER JOIN PUBLIC.DISCENTE                  D  ON D.ID_DISCENTE=M.ID_DISCENTE
 LEFT  JOIN PUBLIC.CURSO                     C  ON C.ID_CURSO=D.ID_CURSO
 INNER JOIN COMUM.PESSOA                    PE ON PE.ID_PESSOA=D.ID_PESSOA
 WHERE (
        (PP.COD_ANO >= $P{anoInicio})
        AND 
        (PP.COD_ANO <= $P{anoFim})
       )
   AND S.ID_SERVIDOR=$P{id_servidor}
 ORDER BY PP.COD_ANO,PE.NOME,C.NOME]]></queryString>

