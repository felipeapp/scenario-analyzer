	</parameter>
	<queryString><![CDATA[SELECT S.ID_UNIDADE,T.ANO,P.NOME,
/*       AVG(CCD.CR_AULA+CCD.CR_LABORATORIO+CCD.CR_ESTAGIO) AS MEDIA*/
/*       SUM(CCD.CR_AULA+CCD.CR_LABORATORIO+CCD.CR_ESTAGIO)/2 AS MEDIA*/
       SUM(dt.ch_dedicada_periodo/15)/(SELECT CASE
                                                WHEN $P{periodo}=-1 THEN 2.0
                                                ELSE 1
                                              END 
                                         FROM ENSINO.TURMA LIMIT 1
                                      ) AS MEDIA,
       SUM(dt.ch_dedicada_periodo)/(SELECT CASE
                                                WHEN $P{periodo}=-1 THEN 2.0
                                                ELSE 1
                                              END 
                                         FROM ENSINO.TURMA LIMIT 1
                                      ) AS CH_MEDIA
  FROM ENSINO.TURMA T
 INNER JOIN ENSINO.DOCENTE_TURMA         DT ON DT.ID_TURMA=T.ID_TURMA
 INNER JOIN RH.SERVIDOR                   S ON S.ID_SERVIDOR=DT.ID_DOCENTE
 INNER JOIN COMUM.PESSOA                 P ON P.ID_PESSOA=S.ID_PESSOA
 INNER JOIN ENSINO.COMPONENTE_CURRICULAR CC ON CC.ID_DISCIPLINA=T.ID_DISCIPLINA
 INNER JOIN ENSINO.COMPONENTE_CURRICULAR_DETALHES CCD ON CCD.ID_COMPONENTE_DETALHES=CC.ID_DETALHE
 WHERE S.ID_UNIDADE=$P{unidade}
   AND T.ANO=$P{ano}
   AND (T.ID_POLO IS NULL)
   AND ($P{periodo}=-1 OR (($P{periodo}=1 and T.PERIODO IN (1,3)) OR ($P{periodo}=2 and T.PERIODO IN (2,4))))
   AND CC.NIVEL='S' /* G=Graduacao S=Pós-graduacao */
 GROUP BY S.ID_UNIDADE,T.ANO,P.NOME
 ORDER BY P.NOME]]></queryString>

