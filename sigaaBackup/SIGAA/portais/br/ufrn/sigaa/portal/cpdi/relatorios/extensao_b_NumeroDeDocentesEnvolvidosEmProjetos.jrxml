	<queryString><![CDATA[SELECT DISTINCT PE.NOME 
  FROM EXTENSAO.ATIVIDADE                A
  INNER JOIN PROJETOS.MEMBRO_PROJETO     MP ON MP.ID_PROJETO=A.ID_PROJETO
  INNER JOIN PROJETOS.PROJETO               P ON P.ID_PROJETO=A.ID_PROJETO
  INNER JOIN RH.SERVIDOR                  S ON S.ID_SERVIDOR=MP.ID_SERVIDOR
  INNER JOIN COMUM.PESSOA               PE ON PE.ID_PESSOA=S.ID_PESSOA
  WHERE P.ANO BETWEEN $P{anoInicio} AND $P{anoFim}
    AND P.ID_TIPO_SITUACAO_PROJETO NOT IN (104,108,107,111,304,306,313,316,318) 
/*    AND MP.ID_FUNCAO_MEMBRO=1 */ /* COORDENADOR */
