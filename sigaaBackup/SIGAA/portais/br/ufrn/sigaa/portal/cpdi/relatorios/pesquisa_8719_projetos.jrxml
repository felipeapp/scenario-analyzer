	<queryString><![CDATA[SELECT DISTINCT U.ID_UNIDADE AS ID_UNIDADE,PP.COD_ANO AS ANO,
                CASE 
                 WHEN MP.ID_FUNCAO_MEMBRO IS NULL THEN
                      CASE 
                         WHEN MP.TIPO_PARTICIPACAO=1 THEN 'COORDENADOR(A)'
                         WHEN MP.TIPO_PARTICIPACAO=2 THEN 'VICE-COORDENADOR(A)'
                         WHEN MP.TIPO_PARTICIPACAO=3 THEN 'COLABORADOR'
                         ELSE 'NÃO INFORMADO'
                      END
                 ELSE F.DESCRICAO
                END AS FUNCAO,
                PP.COD_NUMERO,PJ.TITULO
  FROM PESQUISA.PROJETO_PESQUISA             PP
 INNER JOIN PROJETOS.PROJETO                   PJ ON PJ.ID_PROJETO=PP.ID_PROJETO
 INNER JOIN PROJETOS.MEMBRO_PROJETO          MP ON MP.ID_PROJETO_PESQUISA=PP.ID_PROJETO
 LEFT  JOIN PROJETOS.FUNCAO_MEMBRO           F  ON F.ID_FUNCAO_MEMBRO=MP.ID_FUNCAO_MEMBRO
 INNER JOIN COMUM.PESSOA                    PE ON PE.ID_PESSOA=MP.ID_PESSOA
 INNER JOIN RH.SERVIDOR                      S  ON S.ID_PESSOA=PE.ID_PESSOA
 INNER JOIN PESQUISA.PLANO_TRABALHO          P  ON P.ID_PROJETO_PESQUISA=PP.ID_PROJETO
 INNER JOIN COMUM.UNIDADE                   U  ON U.ID_UNIDADE=S.ID_UNIDADE 
 WHERE (
        (PP.COD_ANO >= $P{anoInicio})
        AND 
        (PP.COD_ANO <= $P{anoFim})
       )
   AND S.ID_SERVIDOR=$P{id_servidor}
 ORDER BY PP.COD_ANO,PP.COD_NUMERO,PJ.TITULO]]></queryString>

