	<parameter name="anoFim" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(2007)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT DISTINCT U.ID_UNIDADE AS ID_UNIDADE,PE.NOME
 FROM PESQUISA.PROJETO_PESQUISA             PP
 INNER JOIN PROJETOS.PROJETO                 PJ ON PJ.ID_PROJETO=PP.ID_PROJETO
 INNER JOIN PROJETOS.MEMBRO_PROJETO          MP ON MP.ID_PROJETO=PJ.ID_PROJETO
 INNER JOIN RH.SERVIDOR                      S  ON S.ID_SERVIDOR=MP.ID_SERVIDOR
 INNER JOIN COMUM.PESSOA                     PE ON PE.ID_PESSOA=S.ID_PESSOA
 INNER JOIN COMUM.UNIDADE                    U  ON U.ID_UNIDADE=S.ID_UNIDADE 
 WHERE PP.COD_ANO BETWEEN $P{anoInicio} AND $P{anoFim}
	  AND S.ID_UNIDADE=$P{unidade}
  AND S.ID_CATEGORIA=1 /*DOCENTE*/
 ORDER BY PE.NOME]]></queryString>

