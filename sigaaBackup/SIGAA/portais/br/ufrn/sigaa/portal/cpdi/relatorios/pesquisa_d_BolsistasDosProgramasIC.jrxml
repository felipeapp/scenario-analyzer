	<queryString><![CDATA[SELECT DISTINCT $P{unidade} AS ID_UNIDADE,D.ID_DISCENTE,PP.COD_ANO AS ANO,D.MATRICULA,C.NOME AS CURSONOME,PE.NOME
FROM PESQUISA.MEMBRO_PROJETO_DISCENTE M
  INNER JOIN PESQUISA.PLANO_TRABALHO          P ON P.ID_PLANO_TRABALHO=M.ID_PLANO_TRABALHO
  INNER JOIN PESQUISA.PROJETO_PESQUISA        PP ON PP.ID_PROJETO_PESQUISA=P.ID_PROJETO_PESQUISA
  INNER JOIN PUBLIC.DISCENTE                  D ON D.ID_DISCENTE=M.ID_DISCENTE
  LEFT  JOIN PUBLIC.CURSO                     C ON C.ID_CURSO=D.ID_CURSO
  INNER JOIN COMUM.PESSOA                     PE ON PE.ID_PESSOA=D.ID_PESSOA
  WHERE P.ID_PROJETO_PESQUISA IN (
		 SELECT DISTINCT PP.ID_PROJETO_PESQUISA
		 FROM PESQUISA.PROJETO_PESQUISA             PP
		 INNER JOIN PROJETOS.PROJETO                 PJ ON PJ.ID_PROJETO=PP.ID_PROJETO
		 INNER JOIN PROJETOS.MEMBRO_PROJETO          MP ON MP.ID_PROJETO=PJ.ID_PROJETO
		 INNER JOIN RH.SERVIDOR                      S  ON S.ID_SERVIDOR=MP.ID_SERVIDOR
		 INNER JOIN COMUM.PESSOA                     PE ON PE.ID_PESSOA=S.ID_PESSOA
		 INNER JOIN COMUM.UNIDADE                    U  ON U.ID_UNIDADE=S.ID_UNIDADE                    WHERE PP.COD_ANO BETWEEN $P{anoInicio} AND $P{anoFim}
                     AND S.ID_UNIDADE=$P{unidade}
                     AND (MP.TIPO_PARTICIPACAO = 1 OR ID_FUNCAO_MEMBRO = 1)
)
 ORDER BY PP.COD_ANO,PE.NOME,C.NOME]]></queryString>

